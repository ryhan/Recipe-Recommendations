DATA = "['{"name": "Basil Chicken over Angel Hair", "ingredients": [{"item": "angel hair pasta", "amount": "1 (8 ounce) package"}, {"item": "olive oil", "amount": "2 teaspoons"}, {"item": "finely chopped onion", "amount": "1/2 cup"}, {"item": "garlic, chopped", "amount": "1 clove"}, {"item": "chopped tomatoes", "amount": "2 1/2 cups"}, {"item": "boneless chicken breast halves, cooked and cubed", "amount": "2 cups"}, {"item": "chopped fresh basil", "amount": "1/4 cup"}, {"item": "salt", "amount": "1/2 teaspoon"}, {"item": "hot pepper sauce", "amount": "1/8 teaspoon"}, {"item": "Parmesan cheese", "amount": "1/4 cup"}], "url": "http://allrecipes.com/Recipe/Basil-Chicken-over-Angel-Hair/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/03/94/39411.jpg", "ready_in": "35  mins", "directions": ["In a large pot of salted boiling water, cook angel hair pasta until it is al dente, about 8 to 10 minutes. Drain, and set aside.", "In a large skillet, heat oil over medium-high heat. Saute the onions and garlic. Stir in the tomatoes, chicken, basil, salt and hot pepper sauce. Reduce heat to medium, and cover skillet. Simmer for about 5 minutes, stirring frequently, until mixture is hot and tomatoes are soft.", "Toss sauce with hot cooked angel hair pasta to coat. Serve with Parmesan cheese."], "description": "\\"A tasty chicken and pasta dish.\\""}', '{"name": "Laura\'s Quick Slow Cooker Turkey Chili", "ingredients": [{"item": "vegetable oil", "amount": "1 tablespoon"}, {"item": "ground turkey", "amount": "1 pound"}, {"item": "low sodium tomato soup", "amount": "2 (10.75 ounce) cans"}, {"item": "kidney beans, drained", "amount": "2 (15 ounce) cans"}, {"item": "black beans, drained", "amount": "1 (15 ounce) can"}, {"item": "onion, chopped", "amount": "1/2 medium"}, {"item": "chili powder", "amount": "2 tablespoons"}, {"item": "red pepper flakes", "amount": "1 teaspoon"}, {"item": "garlic powder", "amount": "1/2 tablespoon"}, {"item": "ground cumin", "amount": "1/2 tablespoon"}, {"item": "ground black pepper", "amount": "1 pinch"}, {"item": "ground allspice", "amount": "1 pinch"}, {"item": "salt to taste", "amount": ""}], "url": "http://allrecipes.com/Recipe/Lauras-Quick-Slow-Cooker-Turkey-Chili/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/01/00/91/1009122.jpg", "ready_in": "4+  hrs", "directions": ["Heat the oil in a skillet over medium heat. Place turkey in the skillet, and cook until evenly brown; drain.", "Coat the inside of a slow cooker with cooking spray, and mix in turkey, tomato soup, kidney beans, black beans and onion. Season with chili powder, red pepper flakes, garlic powder, cumin, black pepper, allspice and salt.", "Cover, and cook 8 hours on Low or 4 hours on High."], "description": "\\"This is an easy chili recipe that you can throw in the slow cooker and forget about all day.  Great topped with Cheddar cheese, crushed corn chips, and a dollop of sour cream.\\""}', '{"name": "Pork Chops for the Slow Cooker", "ingredients": [{"item": "boneless pork chops", "amount": "6"}, {"item": "brown sugar", "amount": "1/4 cup"}, {"item": "ground ginger", "amount": "1 teaspoon"}, {"item": "soy sauce", "amount": "1/2 cup"}, {"item": "ketchup", "amount": "1/4 cup"}, {"item": "garlic, crushed", "amount": "2 cloves"}, {"item": "salt and pepper to taste", "amount": ""}], "url": "http://allrecipes.com/Recipe/Pork-Chops-for-the-Slow-Cooker/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/23/91/239145.jpg", "ready_in": "6+  hrs", "directions": ["Place pork chops in slow cooker. Combine remaining ingredients and pour over pork chops.", "Cook on Low setting for 6 hours, until internal temperature of pork has reached 145 degrees F (63 degrees C)."], "description": "\\"As with most slow cooker recipes this is tender and delicious.\\""}', '{"name": "White Bean Chicken Chili", "ingredients": [{"item": "vegetable oil", "amount": "2 tablespoons"}, {"item": "onion, chopped", "amount": "1"}, {"item": "garlic, minced", "amount": "2 cloves"}, {"item": "\\u00a0", "amount": ""}, {"item": "chicken broth", "amount": "1 (14.5 ounce) can"}, {"item": "tomatillos, drained and chopped", "amount": "1 (18.75 ounce) can"}, {"item": "diced tomatoes", "amount": "1 (16 ounce) can"}, {"item": "diced green chiles", "amount": "1 (7 ounce) can"}, {"item": "dried oregano", "amount": "1/2 teaspoon"}, {"item": "ground coriander seed", "amount": "1/2 teaspoon"}, {"item": "ground cumin", "amount": "1/4 teaspoon"}, {"item": "fresh corn", "amount": "2 ears"}, {"item": "diced, cooked chicken meat", "amount": "1 pound"}, {"item": "white beans", "amount": "1 (15 ounce) can"}, {"item": "salt and black pepper to taste", "amount": "1 pinch"}], "url": "http://allrecipes.com/Recipe/White-Bean-Chicken-Chili/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/12/16/121690.jpg", "ready_in": "35  mins", "directions": ["Heat oil, and cook onion and garlic until soft.", "Stir in broth, tomatillos, tomatoes, chilies, and spices.  Bring to a boil, then simmer for 10 minutes.", "Add corn, chicken, and beans; simmer 5 minutes.  Season with salt and pepper to taste."], "description": "\\"This recipe was passed down from a friend, and now it is one of our favorites!  It\'s great to make whenever there is left over chicken or turkey around!  We pray for colder weather in California just so we can make this soup!  You can substitute additional diced tomatoes for the tomatillos. If you would like the soup to be spicier, you can add some red chili flakes or salsa. Serve with these toppings for people to choose from: limes, cilantro,  cheese, avocado, sour cream, and tortilla chips.\\""}', '{"name": "Easy Baked Tilapia", "ingredients": [{"item": "tilapia", "amount": "4 (4 ounce) fillets"}, {"item": "butter", "amount": "2 teaspoons"}, {"item": "Old Bay Seasoning TM, or to taste", "amount": "1/4 teaspoon"}, {"item": "garlic salt, or to taste", "amount": "1/2 teaspoon"}, {"item": "lemon, sliced", "amount": "1"}, {"item": "frozen cauliflower with broccoli and red pepper", "amount": "1 (16 ounce) package"}], "url": "http://allrecipes.com/Recipe/Easy-Baked-Tilapia/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/01/10/16/1101614.jpg", "ready_in": "35  mins", "directions": ["Preheat the oven to 375 degrees F (190 degrees F). Grease a 9x13 inch baking dish.", "Place the tilapia fillets in the bottom of the baking dish and dot with butter. Season with Old Bay seasoning and garlic salt. Top each one with a slice or two of lemon. Arrange the frozen mixed vegetables around the fish, and season lightly with salt and pepper.", "Cover the dish and bake for 25 to 30 minutes in the preheated oven, until vegetables are tender and fish flakes easily with a fork."], "description": "\\"This easy recipe for tilapia only takes a few minutes to prepare, uses few ingredients, and is flavorful.\\""}', '{"name": "Penne Pasta with Spinach and Bacon", "ingredients": [{"item": "penne pasta", "amount": "1 (12 ounce) package"}, {"item": "olive oil, divided", "amount": "2 tablespoons"}, {"item": "bacon, chopped", "amount": "6 slices"}, {"item": "minced garlic", "amount": "2 tablespoons"}, {"item": "diced tomatoes", "amount": "1 (14.5 ounce) can"}, {"item": "fresh spinach, rinsed and torn into bite-size pieces", "amount": "1 bunch"}], "url": "http://allrecipes.com/Recipe/Penne-Pasta-with-Spinach-and-Bacon/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/63/84/638486.jpg", "ready_in": "25  mins", "directions": ["Bring a large pot of lightly salted water to a boil. Add the penne pasta, and cook until tender, 8 to 10 minutes.", "Meanwhile, heat 1 tablespoon of olive oil in a skillet over medium heat. Place bacon in the skillet, and cook until browned and crisp. Add garlic, and cook for about 1 minute.  Stir in the tomatoes, and cook until heated through.", "Place the spinach into a colander, and drain the hot pasta over it so it is wilted. Transfer to a large serving bowl, and toss with the remaining olive oil, and the bacon and tomato mixture."], "description": "\\"This is a quick light meal. Wilted spinach, bacon and tomatoes are tossed with penne pasta. Good for any season and will complement anything.\\""}', '{"name": "Grilled Fish Tacos with Chipotle-Lime Dressing", "ingredients": [{"item": "extra virgin olive oil", "amount": "1/4 cup"}, {"item": "distilled white vinegar", "amount": "2 tablespoons"}, {"item": "fresh lime juice", "amount": "2 tablespoons"}, {"item": "lime zest", "amount": "2 teaspoons"}, {"item": "honey", "amount": "1 1/2 teaspoons"}, {"item": "garlic, minced", "amount": "2 cloves"}, {"item": "cumin", "amount": "1/2 teaspoon"}, {"item": "chili powder", "amount": "1/2 teaspoon"}, {"item": "seafood seasoning, such as Old Bay\\u2122", "amount": "1 teaspoon"}, {"item": "ground black pepper", "amount": "1/2 teaspoon"}, {"item": "hot pepper sauce, or to taste", "amount": "1 teaspoon"}, {"item": "tilapia fillets, cut into chunks", "amount": "1 pound"}, {"item": "\\u00a0", "amount": ""}, {"item": "light sour cream", "amount": "1 (8 ounce) container"}, {"item": "adobo sauce from chipotle peppers", "amount": "1/2 cup"}, {"item": "fresh lime juice", "amount": "2 tablespoons"}, {"item": "lime zest", "amount": "2 teaspoons"}, {"item": "cumin", "amount": "1/4 teaspoon"}, {"item": "chili powder", "amount": "1/4 teaspoon"}, {"item": "seafood seasoning, such as Old Bay\\u2122", "amount": "1/2 teaspoon"}, {"item": "salt and pepper to taste", "amount": ""}, {"item": "\\u00a0", "amount": ""}, {"item": "tortillas", "amount": "1 (10 ounce) package"}, {"item": "ripe tomatoes, seeded and diced", "amount": "3"}, {"item": "cilantro, chopped", "amount": "1 bunch"}, {"item": "cabbage, cored and shredded", "amount": "1 small head"}, {"item": "limes, cut in wedges", "amount": "2"}], "url": "http://allrecipes.com/Recipe/Grilled-Fish-Tacos-with-Chipotle-Lime-Dressing/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/66/45/664505.jpg", "ready_in": "6+  hrs", "directions": ["To make the marinade, whisk together the olive oil, vinegar, lime juice, lime zest, honey, garlic, cumin, chili powder, seafood seasoning, black pepper, and hot sauce in a bowl until blended. Place the tilapia in a shallow dish, and pour the marinade over the fish. Cover, and refrigerate 6 to 8 hours.", "To make the dressing, combine the sour cream and adobo sauce in a bowl. Stir in the lime juice, lime zest, cumin, chili powder, seafood seasoning. Add salt, and pepper in desired amounts. Cover, and refrigerate until needed.", "Preheat an outdoor grill for high heat and lightly oil grate. Set grate 4 inches from the heat.", "Remove fish from marinade, drain off any excess and discard marinade. Grill fish pieces until easily flaked with a fork, turning once, about 9 minutes.", "Assemble tacos by placing fish pieces in the center of tortillas with desired amounts of tomatoes, cilantro, and cabbage; drizzle with dressing. To serve, roll up tortillas around fillings, and garnish with lime wedges."], "description": "\\"I came up with this recipe when I couldn\'t find good directions for grilled fish tacos. I liked the chipotle-lime combination in a dip I once tried so I imitated it here. Choose toppings to suit your taste, or include typical accompaniments such as salsa fresca, cabbage, a squeeze of lime juice, and chopped cilantro.\\""}', '{"name": "Grandma\'s Slow Cooker Vegetarian Chili", "ingredients": [{"item": "black bean soup", "amount": "1 (19 ounce) can"}, {"item": "kidney beans, rinsed and drained", "amount": "1 (15 ounce) can"}, {"item": "garbanzo beans, rinsed and drained", "amount": "1 (15 ounce) can"}, {"item": "vegetarian baked beans", "amount": "1 (16 ounce) can"}, {"item": "chopped tomatoes in puree", "amount": "1 (14.5 ounce) can"}, {"item": "whole kernel corn, drained", "amount": "1 (15 ounce) can"}, {"item": "onion, chopped", "amount": "1"}, {"item": "green bell pepper, chopped", "amount": "1"}, {"item": "celery, chopped", "amount": "2 stalks"}, {"item": "garlic, chopped", "amount": "2 cloves"}, {"item": "chili powder, or to taste", "amount": "1 tablespoon"}, {"item": "dried parsley", "amount": "1 tablespoon"}, {"item": "dried oregano", "amount": "1 tablespoon"}, {"item": "dried basil", "amount": "1 tablespoon"}], "url": "http://allrecipes.com/Recipe/Grandmas-Slow-Cooker-Vegetarian-Chili/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/04/57/45784.jpg", "ready_in": "2\\u00bc  hrs", "directions": ["In a slow cooker, combine black bean soup, kidney beans, garbanzo beans, baked beans, tomatoes, corn, onion, bell pepper and celery. Season with garlic, chili powder, parsley, oregano and basil. Cook for at least two hours on High."], "description": "\\"This is a simple crowd pleasing recipe that can sit in a slow cooker until it is time to serve.\\""}', '{"name": "Fiery Fish Tacos with Crunchy Corn Salsa", "ingredients": [{"item": "corn", "amount": "1 cup"}, {"item": "diced red onion", "amount": "1/2 cup"}, {"item": "peeled, chopped jicama", "amount": "1 cup"}, {"item": "diced red bell pepper", "amount": "1/2 cup"}, {"item": "fresh cilantro leaves, finely chopped", "amount": "1 cup"}, {"item": "lime, zested and juiced", "amount": "1"}, {"item": "sour cream", "amount": "2 tablespoons"}, {"item": "cayenne pepper", "amount": "2 tablespoons"}, {"item": "ground black pepper", "amount": "1 tablespoon"}, {"item": "salt", "amount": "2 tablespoons"}, {"item": "tilapia", "amount": "6 (4 ounce) fillets"}, {"item": "olive oil", "amount": "2 tablespoons"}, {"item": "corn tortillas, warmed", "amount": "12"}], "url": "http://allrecipes.com/Recipe/Fiery-Fish-Tacos-with-Crunchy-Corn-Salsa/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/28/98/289814.jpg", "ready_in": "40  mins", "directions": ["Preheat grill for high heat.", "In a medium bowl, mix together corn, red onion, jicama, red bell pepper, and cilantro. Stir in lime juice and zest.", "In a small bowl, combine cayenne pepper, ground black pepper, and salt.  Brush each fillet with olive oil, and sprinkle with spices.", "Arrange fillets on grill grate, and cook for 3 minutes per side. For each fiery fish taco, top two corn tortillas with fish, sour cream, and corn salsa."], "description": "\\"Spicy grilled fish are cooled down with a fresh crunchy veggie salsa featuring fresh corn. Your guests will swim back for seconds!\\""}', '{"name": "Pineapple Chicken Tenders", "ingredients": [{"item": "pineapple juice", "amount": "1 cup"}, {"item": "packed brown sugar", "amount": "1/2 cup"}, {"item": "light soy sauce", "amount": "1/3 cup"}, {"item": "chicken breast tenderloins or strips", "amount": "2 pounds"}, {"item": "skewers", "amount": ""}], "url": "http://allrecipes.com/Recipe/Pineapple-Chicken-Tenders/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/22/74/227447.jpg", "ready_in": "1\\u00bc  hrs", "directions": ["In a small saucepan over medium heat, mix pineapple juice, brown sugar, and soy sauce. Remove from heat just before the mixture comes to a boil.", "Place chicken tenders in a medium bowl. Cover with the pineapple marinade, and refrigerate for at least 30 minutes.", "Preheat grill for medium heat. Thread chicken lengthwise onto wooden skewers.", "Lightly oil the grill grate. Grill chicken tenders 5 minutes per side, or until juices run clear. They cook quickly, so watch them closely."], "description": "\\"Delicious little bites for an appetizer or a light meal with a salad!\\""}', '{"name": "Andrea\'s Pasta Fagioli", "ingredients": [{"item": "olive oil", "amount": "3 tablespoons"}, {"item": "onion, quartered then halved", "amount": "1"}, {"item": "garlic, minced", "amount": "2 cloves"}, {"item": "tomato sauce", "amount": "1 (29 ounce) can"}, {"item": "water", "amount": "5 1/2 cups"}, {"item": "dried parsley", "amount": "1 tablespoon"}, {"item": "dried basil", "amount": "1 1/2 teaspoons"}, {"item": "dried oregano", "amount": "1 1/2 teaspoons"}, {"item": "salt", "amount": "1 teaspoon"}, {"item": "cannellini beans", "amount": "1 (15 ounce) can"}, {"item": "navy beans", "amount": "1 (15 ounce) can"}, {"item": "grated Parmesan cheese", "amount": "1/3 cup"}, {"item": "ditalini pasta", "amount": "1 pound"}], "url": "http://allrecipes.com/Recipe/Andreas-Pasta-Fagioli/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/22/36/223672.jpg", "ready_in": "1\\u00be  hrs", "directions": ["In a large pot over medium heat, cook onion in olive oil until translucent.  Stir in garlic and cook until tender.  Reduce heat, and stir in tomato sauce, water, parsley, basil, oregano, salt, cannelini beans, navy beans and Parmesan.  Simmer 1 hour.", "Bring a large pot of lightly salted water to a boil. Add pasta and cook for 8 to 10 minutes or until al dente; drain.  Stir into soup."], "description": "\\"This is a recipe I have adapted a bit from my grandmother\'s.  It\'s easy and perfect for a Sunday afternoon with fresh Italian bread.\\""}', '{"name": "Pesto Pasta with Chicken", "ingredients": [{"item": "bow tie pasta", "amount": "1 (16 ounce) package"}, {"item": "olive oil", "amount": "1 teaspoon"}, {"item": "garlic, minced", "amount": "2 cloves"}, {"item": "boneless skinless chicken breasts, cut into bite-size pieces", "amount": "2"}, {"item": "crushed red pepper flakes to taste", "amount": ""}, {"item": "oil-packed sun-dried tomatoes, drained and cut into strips", "amount": "1/3 cup"}, {"item": "pesto sauce", "amount": "1/2 cup"}], "url": "http://allrecipes.com/Recipe/Pesto-Pasta-with-Chicken/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/59/38/593876.jpg", "ready_in": "30  mins", "directions": ["Bring a large pot of lightly salted water to a boil. Add pasta and cook for 8 to 10 minutes or until al dente; drain.", "Heat oil in a large skillet over medium heat. Saute garlic until tender, then stir in chicken. Season with red pepper flakes. Cook until chicken is golden, and cooked through.", "In a large bowl, combine pasta, chicken, sun-dried tomatoes and pesto. Toss to coat evenly."], "description": "\\"Easy and delicious pasta with chicken. Serve with crusty bread and salad for a quick dinner. Use as much or as little pesto sauce as you like. Using homemade pesto will taste even better, but it adds to prep time. Enjoy!\\""}', '{"name": "Slow Cooker Chicken Cacciatore", "ingredients": [{"item": "skinless, boneless chicken breast halves", "amount": "6"}, {"item": "spaghetti sauce", "amount": "1 (28 ounce) jar"}, {"item": "green bell pepper, seeded and cubed", "amount": "2"}, {"item": "fresh mushrooms, sliced", "amount": "8 ounces"}, {"item": "onion, finely diced", "amount": "1"}, {"item": "minced garlic", "amount": "2 tablespoons"}], "url": "http://allrecipes.com/Recipe/Slow-Cooker-Chicken-Cacciatore/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/48/35/483543.jpg", "ready_in": "9+  hrs", "directions": ["Put the chicken in the slow cooker. Top with the spaghetti sauce, green bell peppers, mushrooms, onion, and garlic.", "Cover, and cook on Low for 7 to 9 hours."], "description": "\\"Easy slow cooker chicken cacciatore. Serve over angel hair pasta. \'Cacciatore\' is Italian for \'hunter\', and this American-Italian term refers to food prepared \'hunter style,\' with mushrooms and onions. Avanti!\\""}', '{"name": "Chicken and Corn Chili", "ingredients": [{"item": "skinless, boneless chicken breast halves", "amount": "4 skinless, boneless chicken breast halves"}, {"item": "salsa", "amount": "1 (16 ounce) jar"}, {"item": "garlic powder", "amount": "2 teaspoons"}, {"item": "ground cumin", "amount": "1 teaspoon"}, {"item": "chili powder", "amount": "1 teaspoon"}, {"item": "salt to taste", "amount": ""}, {"item": "ground black pepper to taste", "amount": ""}, {"item": "Mexican-style corn", "amount": "1 (11 ounce) can"}, {"item": "pinto beans", "amount": "1 (15 ounce) can"}], "url": "http://allrecipes.com/Recipe/Chicken-and-Corn-Chili/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/12/04/120470.jpg", "ready_in": "12+  hrs", "directions": ["Place chicken and salsa in the slow cooker the night before you want to eat this chili. Season with garlic powder, cumin, chili powder, salt, and pepper. Cook 6 to 8 hours on Low setting.", "About 3 to 4 hours before you want to eat, shred the chicken with 2 forks. Return the meat to the pot, and continue cooking.", "Stir the corn and the pinto beans into the slow cooker. Simmer until ready to serve."], "description": "\\"This is an easy slow cooker meal - use your imagination and season it up as you like! Great on a c-c-c-cold winter night! I serve this with grated cheese, sour cream, chopped cilantro and green onions, and flour tortillas on the side.\\""}', '{"name": "Greek Penne and Chicken", "ingredients": [{"item": "penne pasta", "amount": "1 (16 ounce) package"}, {"item": "butter", "amount": "1 1/2 tablespoons"}, {"item": "chopped red onion", "amount": "1/2 cup"}, {"item": "garlic, minced", "amount": "2 cloves"}, {"item": "skinless, boneless chicken breast halves - cut into bite-size pieces", "amount": "1 pound"}, {"item": "artichoke hearts in water", "amount": "1 (14 ounce) can"}, {"item": "tomato, chopped", "amount": "1"}, {"item": "crumbled feta cheese", "amount": "1/2 cup"}, {"item": "chopped fresh parsley", "amount": "3 tablespoons"}, {"item": "lemon juice", "amount": "2 tablespoons"}, {"item": "dried oregano", "amount": "1 teaspoon"}, {"item": "salt to taste", "amount": ""}, {"item": "ground black pepper to taste", "amount": ""}], "url": "http://allrecipes.com/Recipe/Greek-Penne-and-Chicken/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/05/84/58408.jpg", "ready_in": "50  mins", "directions": ["In a large pot with boiling salted water cook penne pasta until al dente. Drain.", "Meanwhile, in a large skillet over medium-high heat melt butter, add onion and garlic and cook for 2 minutes. Add chopped chicken and continue cooking, stirring occasionally until golden brown, about 5 to 6 minutes.", "Reduce heat to medium- low. Drain and chop artichoke hearts and add them, chopped tomato, feta cheese, fresh parsley, lemon juice, dried oregano, and drained penne pasta to the large skillet. Cook until heated through, about 2 to 3 minutes.", "Season with salt and ground black pepper. Serve warm."], "description": "\\"This is one of my favorite \'stand by\' recipes. I keep the ingredients on hand to make a quick, delicious, satisfying meal.\\""}', '{"name": "Southwest Chicken", "ingredients": [{"item": "vegetable oil", "amount": "1 tablespoon"}, {"item": "skinless, boneless chicken breast halves", "amount": "4 skinless, boneless chicken breast halves"}, {"item": "diced tomatoes with green chile peppers", "amount": "1 (10 ounce) can"}, {"item": "black beans, rinsed and drained", "amount": "1 (15 ounce) can"}, {"item": "whole kernel corn, drained", "amount": "1 (8.75 ounce) can"}, {"item": "ground cumin", "amount": "1 pinch"}], "url": "http://allrecipes.com/Recipe/Southwest-Chicken/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/26/80/268039.jpg", "ready_in": "", "directions": ["In a large skillet, heat oil over medium high heat. Brown chicken breasts on both sides. Add tomatoes with green chile peppers, beans and corn. Reduce heat and let simmer for 25 to 30 minutes or until chicken is cooked through and juices run clear. Add a dash of cumin and serve."], "description": "\\"Chicken breasts with black beans, corn, chile peppers, tomatoes. Low-fat, easy and quick. Serve over hot cooked rice if desired.\\""}', '{"name": "Hawaiian Chicken Kabobs", "ingredients": [{"item": "soy sauce", "amount": "3 tablespoons"}, {"item": "brown sugar", "amount": "3 tablespoons"}, {"item": "sherry", "amount": "2 tablespoons"}, {"item": "sesame oil", "amount": "1 tablespoon"}, {"item": "ground ginger", "amount": "1/4 teaspoon"}, {"item": "garlic powder", "amount": "1/4 teaspoon"}, {"item": "skinless, boneless chicken breast halves - cut into 2 inch pieces", "amount": "8"}, {"item": "pineapple chunks, drained", "amount": "1 (20 ounce) can"}, {"item": "skewers", "amount": ""}], "url": "http://allrecipes.com/Recipe/Hawaiian-Chicken-Kabobs/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/39/37/393772.jpg", "ready_in": "2\\u00bd  hrs", "directions": ["In a shallow glass dish, mix the soy sauce, brown sugar, sherry, sesame oil, ginger, and garlic powder. Stir the chicken pieces and pineapple into the marinade until well coated. Cover, and marinate in the refrigerator at least 2 hours.", "Preheat grill to medium-high heat.", "Lightly oil the grill grate. Thread chicken and pineapple alternately onto skewers. Grill 15 to 20 minutes, turning occasionally, or until chicken juices run clear."], "description": "\\"These kabobs are tender, sweet, and delicious. They\'re easy to make and only require a few ingredients.\\""}', '{"name": "Fra Diavolo Sauce With Pasta", "ingredients": [{"item": "olive oil, divided", "amount": "4 tablespoons"}, {"item": "garlic, crushed", "amount": "6 cloves"}, {"item": "whole peeled tomatoes with liquid, chopped", "amount": "3 cups"}, {"item": "salt", "amount": "1 1/2 teaspoons"}, {"item": "crushed red pepper flakes", "amount": "1 teaspoon"}, {"item": "linguine pasta", "amount": "1 (16 ounce) package"}, {"item": "small shrimp, peeled and deveined", "amount": "8 ounces"}, {"item": "bay scallops", "amount": "8 ounces"}, {"item": "chopped fresh parsley", "amount": "1 tablespoon"}], "url": "http://allrecipes.com/Recipe/Fra-Diavolo-Sauce-With-Pasta/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/47/35/473536.jpg", "ready_in": "1  hr", "directions": ["In a large saucepan, heat 2 tablespoons of the olive oil with the garlic over medium heat. When the garlic starts to sizzle, pour in the tomatoes. Season with salt and red pepper. Bring to a boil. Lower the heat, and simmer for 30 minutes, stirring occasionally.", "Meanwhile, bring a large pot of lightly salted water to a boil. Cook pasta for 8 to 10 minutes, or until al dente; drain.", "In a large skillet, heat the remaining 2 tablespoons of olive oil over high heat. Add the shrimp and scallops. Cook for about 2 minutes, stirring frequently, or until the shrimp turn pink. Add shrimp and scallops to the tomato mixture, and stir in the parsley. Cook for 3 to 4 minutes, or until the sauce just begins to bubble. Serve sauce over pasta."], "description": "\\"This sauce includes shrimp and scallops, best served with linguine pasta.\\""}', '{"name": "Mock Tuna Salad", "ingredients": [{"item": "garbanzo beans (chickpeas), drained and mashed", "amount": "1 (19 ounce) can"}, {"item": "mayonnaise", "amount": "2 tablespoons"}, {"item": "spicy brown mustard", "amount": "2 teaspoons"}, {"item": "sweet pickle relish", "amount": "1 tablespoon"}, {"item": "green onions, chopped", "amount": "2"}, {"item": "salt and pepper to taste", "amount": ""}], "url": "http://allrecipes.com/Recipe/Mock-Tuna-Salad/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/01/12/60/1126055.jpg", "ready_in": "20  mins", "directions": ["In a medium bowl, combine garbanzo beans, mayonnaise, mustard, relish, chopped green onions, salt and pepper. Mix well."], "description": "\\"This is a chickpea spread that tastes like tuna salad! No kidding! Great served in a sandwich.\\""}', '{"name": "Zucchini and Potato Bake", "ingredients": [{"item": "zucchini, quartered and cut into large pieces", "amount": "2 medium"}, {"item": "potatoes, peeled and cut into large chunks", "amount": "4 medium"}, {"item": "red bell pepper, seeded and chopped", "amount": "1 medium"}, {"item": "garlic, sliced", "amount": "1 clove"}, {"item": "dry bread crumbs", "amount": "1/2 cup"}, {"item": "olive oil", "amount": "1/4 cup"}, {"item": "paprika to taste", "amount": ""}, {"item": "salt to taste", "amount": ""}, {"item": "ground black pepper to taste", "amount": ""}], "url": "http://allrecipes.com/Recipe/Zucchini-and-Potato-Bake/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/26/99/269912.jpg", "ready_in": "1\\u00bc  hrs", "directions": ["Preheat oven to 400 degrees F (200 degrees C).", "In a medium baking pan, toss together the zucchini, potatoes, red bell pepper, garlic, bread crumbs, and olive oil. Season with paprika, salt, and pepper.", "Bake 1 hour in the preheated oven, stirring occasionally, until potatoes are tender and lightly brown."], "description": "\\"This is a very easy and delicious dish that I like to make as a side dish for my summer barbecues.\\""}', '{"name": "Slow Cooker Chili II", "ingredients": [{"item": "ground beef", "amount": "1 pound"}, {"item": "diced onion", "amount": "3/4 cup"}, {"item": "diced celery", "amount": "3/4 cup"}, {"item": "diced green bell pepper", "amount": "3/4 cup"}, {"item": "garlic, minced", "amount": "2 cloves"}, {"item": "tomato puree", "amount": "2 (10.75 ounce) cans"}, {"item": "kidney beans with liquid", "amount": "1 (15 ounce) can"}, {"item": "kidney beans, drained", "amount": "1 (15 ounce) can"}, {"item": "cannellini beans with liquid", "amount": "1 (15 ounce) can"}, {"item": "chili powder", "amount": "1/2 tablespoon"}, {"item": "dried parsley", "amount": "1/2 teaspoon"}, {"item": "salt", "amount": "1 teaspoon"}, {"item": "dried basil", "amount": "3/4 teaspoon"}, {"item": "dried oregano", "amount": "3/4 teaspoon"}, {"item": "ground black pepper", "amount": "1/4 teaspoon"}, {"item": "hot pepper sauce", "amount": "1/8 teaspoon"}], "url": "http://allrecipes.com/Recipe/Slow-Cooker-Chili-II/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/19/66/196638.jpg", "ready_in": "8+  hrs", "directions": ["Place the beef in a skillet over medium heat, and cook until evenly brown. Drain grease.", "Place the beef in a slow cooker, and mix in onion, celery, green bell pepper, garlic, tomato puree, kidney beans, and cannellini beans. Season with chili powder, parsley, salt, basil, oregano, black pepper, and hot pepper sauce.", "Cover, and cook 8 hours on Low."], "description": "\\"The slow cooker is a great way to make chili. This beefy chili is packed with beans, veggies, and spice.\\""}', '{"name": "Chickpea Curry", "ingredients": [{"item": "vegetable oil", "amount": "2 tablespoons"}, {"item": "onions, minced", "amount": "2"}, {"item": "garlic, minced", "amount": "2 cloves"}, {"item": "fresh ginger root, finely chopped", "amount": "2 teaspoons"}, {"item": "whole cloves", "amount": "6"}, {"item": "cinnamon, crushed", "amount": "2 (2 inch) sticks"}, {"item": "ground cumin", "amount": "1 teaspoon"}, {"item": "ground coriander", "amount": "1 teaspoon"}, {"item": "salt", "amount": ""}, {"item": "cayenne pepper", "amount": "1 teaspoon"}, {"item": "ground turmeric", "amount": "1 teaspoon"}, {"item": "garbanzo beans", "amount": "2 (15 ounce) cans"}, {"item": "chopped fresh cilantro", "amount": "1 cup"}], "url": "http://allrecipes.com/Recipe/Chickpea-Curry/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/16/33/163375.jpg", "ready_in": "40  mins", "directions": ["Heat oil in a large frying pan over medium heat, and fry onions until tender.", "Stir in garlic, ginger, cloves, cinnamon, cumin, coriander, salt, cayenne, and turmeric. Cook for 1 minute over medium heat, stirring constantly. Mix in garbanzo beans and their liquid. Continue to cook and stir until all ingredients are well blended and heated through. Remove from heat. Stir in cilantro just before serving, reserving 1 tablespoon for garnish."], "description": "\\"We usually recommend preparing the beans at home, but using canned chickpeas allows for a fast, convenient dish.\\""}', '{"name": "Chicken Fiesta Salad", "ingredients": [{"item": "skinless, boneless chicken breast halves", "amount": "2 skinless, boneless chicken breast halves"}, {"item": "dry fajita seasoning, divided", "amount": "1 (1.27 ounce) packet"}, {"item": "vegetable oil", "amount": "1 tablespoon"}, {"item": "black beans, rinsed and drained", "amount": "1 (15 ounce) can"}, {"item": "Mexican-style corn", "amount": "1 (11 ounce) can"}, {"item": "salsa", "amount": "1/2 cup"}, {"item": "mixed salad greens", "amount": "1 (10 ounce) package"}, {"item": "onion, chopped", "amount": "1"}, {"item": "tomato, cut into wedges", "amount": "1"}], "url": "http://allrecipes.com/Recipe/Chicken-Fiesta-Salad/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/73/20/732063.jpg", "ready_in": "40  mins", "directions": ["Rub chicken evenly with 1/2 the fajita seasoning. Heat the oil in a skillet over medium heat, and cook the chicken 8 minutes on each side, or until juices run clear; set aside.", "In a large saucepan, mix beans, corn, salsa and other 1/2 of fajita seasoning. Heat over medium heat until warm.", "Prepare the salad by tossing the greens, onion and tomato. Top salad with chicken and dress with the bean and corn mixture."], "description": "\\"This is an attractive and zesty all in one dish. It\'s full of veggies and chicken, and loaded with flavor. A quick fix for a long day. If desired, top with shredded cheese and tortilla chips.\\""}', '{"name": "Garlic Chicken with Orzo Noodles", "ingredients": [{"item": "uncooked orzo pasta", "amount": "1 cup"}, {"item": "olive oil", "amount": "2 tablespoons"}, {"item": "garlic", "amount": "2 cloves"}, {"item": "crushed red pepper", "amount": "1/4 teaspoon"}, {"item": "skinless, boneless chicken breast halves - cut into bite-size pieces", "amount": "2"}, {"item": "salt to taste", "amount": ""}, {"item": "chopped fresh parsley", "amount": "1 tablespoon"}, {"item": "fresh spinach leaves", "amount": "2 cups"}, {"item": "grated Parmesan cheese for topping", "amount": ""}], "url": "http://allrecipes.com/Recipe/Garlic-Chicken-with-Orzo-Noodles/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/00/22/2284.jpg", "ready_in": "30  mins", "directions": ["Bring a large pot of lightly salted water to a boil. Add orzo pasta, cook for 8 to 10 minutes, until al dente, and drain.", "Heat the oil in a skillet over medium-high heat, and cook the garlic and red pepper 1 minute, until garlic is golden brown. Stir in chicken, season with salt, and cook 2 to 5 minutes, until lightly browned and juices run clear. Reduce heat to medium, and mix in the parsley and cooked orzo. Place spinach in the skillet. Continue cooking 5 minutes, stirring occasionally, until spinach is wilted. Serve topped with Parmesan cheese."], "description": "\\"This is a simple recipe with a spicy kick for garlic lovers. It is my combination of a much loved linguine and clam sauce dish and a recent introduction to orzo pasta. I have several variations for this recipe.  Add more red pepper for extra spice.  Substitute the chicken for clams or shrimp.  Substitute the spinach leaves for diced tomatoes.  I\'ve also added spices such as basil, rosemary and oregano bringing a distinct Italian flavor to the dish.\\""}', '{"name": "Cod with Italian Crumb Topping", "ingredients": [{"item": "fine dry bread crumbs", "amount": "1/4 cup"}, {"item": "grated Parmesan cheese", "amount": "2 tablespoons"}, {"item": "cornmeal", "amount": "1 tablespoon"}, {"item": "olive oil", "amount": "1 teaspoon"}, {"item": "Italian seasoning", "amount": "1/2 teaspoon"}, {"item": "garlic powder", "amount": "1/8 teaspoon"}, {"item": "ground black pepper", "amount": "1/8 teaspoon"}, {"item": "cod fillets", "amount": "4 (3 ounce) fillets"}, {"item": "egg white, lightly beaten", "amount": "1"}], "url": "http://allrecipes.com/Recipe/Cod-with-Italian-Crumb-Topping/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/92/54/925457.jpg", "ready_in": "25  mins", "directions": ["Preheat oven to 450 degrees F (230 degrees C).", "In a small shallow bowl, stir together the bread crumbs, cheese, cornmeal, oil, italian seasoning, garlic powder and pepper; set aside.", "Coat the rack of a broiling pan with cooking spray. Place the cod on the rack, folding under any thin edges of the filets. Brush with the egg white, then spoon the crumb mixture evenly on top.", "Bake in a preheated oven for 10 to 12 minutes or until the fish flakes easily when tested with a fork and is opaque all the way through."], "description": "\\"This recipe is a good, low-fat, and quick cod recipe.\\""}', '{"name": "Greek Pasta with Tomatoes and White Beans", "ingredients": [{"item": "Italian-style diced tomatoes", "amount": "2 (14.5 ounce) cans"}, {"item": "cannellini beans, drained and rinsed", "amount": "1 (19 ounce) can"}, {"item": "fresh spinach, washed and chopped", "amount": "10 ounces"}, {"item": "penne pasta", "amount": "8 ounces"}, {"item": "crumbled feta cheese", "amount": "1/2 cup"}], "url": "http://allrecipes.com/Recipe/Greek-Pasta-with-Tomatoes-and-White-Beans/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/32/69/326923.jpg", "ready_in": "25  mins", "directions": ["Cook the pasta in a large pot of boiling salted water until al dente.", "Meanwhile, combine tomatoes and beans in a large non-stick skillet.  Bring to a boil over medium high heat.  Reduce heat, and simmer 10 minutes.", "Add spinach to the sauce; cook for 2 minutes or until spinach wilts, stirring constantly.", "Serve sauce over pasta, and sprinkle with feta."], "description": "\\"An easy, quick, and tasty recipe. The flavors are wonderfully different as they are combined and meld together.\\""}', '{"name": "Spicy Chicken Breasts", "ingredients": [{"item": "paprika", "amount": "2 1/2 tablespoons"}, {"item": "garlic powder", "amount": "2 tablespoons"}, {"item": "salt", "amount": "1 tablespoon"}, {"item": "onion powder", "amount": "1 tablespoon"}, {"item": "dried thyme", "amount": "1 tablespoon"}, {"item": "ground cayenne pepper", "amount": "1 tablespoon"}, {"item": "ground black pepper", "amount": "1 tablespoon"}, {"item": "skinless, boneless chicken breast halves", "amount": "4 skinless, boneless chicken breast halves"}], "url": "http://allrecipes.com/Recipe/Spicy-Chicken-Breasts/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/25/37/253758.jpg", "ready_in": "30  mins", "directions": ["In a medium bowl, mix together the paprika, garlic powder, salt, onion powder, thyme, cayenne pepper, and ground black pepper. Set aside about 3 tablespoons of this seasoning mixture for the chicken; store the remainder in an airtight container for later use (for seasoning fish, meats, or vegetables).", "Preheat grill for medium-high heat. Rub some of the reserved 3 tablespoons of seasoning onto both sides of the chicken breasts.", "Lightly oil the grill grate. Place chicken on the grill, and cook for 6 to 8 minutes on each side, until juices run clear."], "description": "\\"This is a great skinless, chicken breast recipe that can be served over salad greens or as an entree! If serving over salad greens, cut chicken into strips and top with your favorite salsa or dressing.\\""}', '{"name": "White Chili I", "ingredients": [{"item": "olive oil", "amount": "1 tablespoon"}, {"item": "skinless, boneless chicken breast halves - cubed", "amount": "4"}, {"item": "onion, chopped", "amount": "1"}, {"item": "chicken broth", "amount": "1 1/4 cups"}, {"item": "diced green chiles", "amount": "1 (4 ounce) can"}, {"item": "garlic powder", "amount": "1 teaspoon"}, {"item": "ground cumin", "amount": "1 teaspoon"}, {"item": "dried oregano", "amount": "1/2 teaspoon"}, {"item": "dried cilantro", "amount": "1/2 teaspoon"}, {"item": "cayenne pepper", "amount": "1/8 teaspoon"}, {"item": "cannellini beans, drained and rinsed", "amount": "1 (15 ounce) can"}, {"item": "green onions, chopped", "amount": "2"}, {"item": "shredded Monterey Jack cheese", "amount": "2 ounces"}], "url": "http://allrecipes.com/Recipe/White-Chili-I/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/92/92/929249.jpg", "ready_in": "35  mins", "directions": ["Heat oil in a large saucepan over medium-high heat. Cook chicken and onion in oil 4 to 5 minutes, or until onion is tender.", "Stir in the chicken broth, green chiles, garlic powder, cumin, oregano, cilantro, and cayenne pepper. Reduce heat, and simmer for 15 minutes.", "Stir in the beans, and simmer for 5 more minutes, or until chicken is no longer pink and juices run clear. Garnish with green onion and shredded cheese."], "description": "\\"Serve with corn bread and salad. If you cube the chicken ahead of time, make corn bread muffins while preparing the chili, and use a bagged salad mix from the produce department, this is a very fast meal to put together.\\""}', '{"name": "Frogmore Stew", "ingredients": [{"item": "water", "amount": "6 quarts"}, {"item": "Old Bay Seasoning TM", "amount": "3/4 cup"}, {"item": "new red potatoes", "amount": "2 pounds"}, {"item": "hot smoked sausage links, cut into 2 inch pieces", "amount": "2 pounds"}, {"item": "corn - husked, cleaned and quartered", "amount": "12 ears"}, {"item": "large fresh shrimp, unpeeled", "amount": "4 pounds"}], "url": "http://allrecipes.com/Recipe/Frogmore-Stew/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/04/87/48715.jpg", "ready_in": "40  mins", "directions": ["Bring water and Old Bay Seasoning to boil in a large stockpot.", "Add potatoes and cook for 15 minutes. Add sausage and cook for 5 minutes more. Add corn and cook for another 5 minutes. Stir in the shrimp and cook until shrimp are pink, about 5 minutes. Drain immediately and serve."], "description": "\\"I\'ve been using this recipe for over 20 years, ever since I moved to South Carolina. It can be adjusted to serve any number. Allow 2 tablespoons of seasoning per quart of water. If you cannot find hot smoked sausage, use another smoked sausage such as kielbasa and add crushed hot red pepper to the pot.\\""}', '{"name": "Spinach and Sun-Dried Tomato Pasta", "ingredients": [{"item": "vegetable broth", "amount": "1 cup"}, {"item": "dehydrated sun-dried tomatoes", "amount": "12"}, {"item": "uncooked penne pasta", "amount": "1 (8 ounce) package"}, {"item": "pine nuts", "amount": "2 tablespoons"}, {"item": "olive oil", "amount": "1 tablespoon"}, {"item": "crushed red pepper flakes", "amount": "1/4 teaspoon"}, {"item": "garlic, minced", "amount": "1 clove"}, {"item": "fresh spinach, rinsed and torn into bite-size pieces", "amount": "1 bunch"}, {"item": "grated Parmesan cheese", "amount": "1/4 cup"}], "url": "http://allrecipes.com/Recipe/Spinach-and-Sun-Dried-Tomato-Pasta/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/21/19/211950.jpg", "ready_in": "40  mins", "directions": ["In a small saucepan, bring the broth to a boil. Remove from heat. Place the sun-dried tomatoes in the broth 15 minutes, or until softened. Drain, reserving broth, and coarsely chop.", "Bring a large pot of lightly salted water to a boil. Place penne pasta in the pot, cook 9 to 12 minutes, until al dente, and drain.", "Place the pine nuts in a skillet over medium heat. Cook and stir until lightly toasted.", "Heat the olive oil and red pepper flakes in a skillet over medium heat, and saute the garlic 1 minute, until tender. Mix in the spinach, and cook until almost wilted. Pour in the reserved broth, and stir in the chopped sun-dried tomatoes. Continue cooking 2 minutes, or until heated through.", "In a large bowl, toss the cooked pasta with the spinach and tomato mixture and pine nuts. Serve with Parmesan cheese."], "description": "\\"I created this simple Sicilian-style pasta dish one day when trying to use up some sun-dried tomatoes.\\""}', '{"name": "Bourbon Chicken", "ingredients": [{"item": "skinless, boneless chicken breast halves", "amount": "4"}, {"item": "ground ginger", "amount": "1 teaspoon"}, {"item": "soy sauce", "amount": "4 ounces"}, {"item": "dried minced onion", "amount": "2 tablespoons"}, {"item": "packed brown sugar", "amount": "1/2 cup"}, {"item": "bourbon", "amount": "3/8 cup"}, {"item": "garlic powder", "amount": "1/2 teaspoon"}], "url": "http://allrecipes.com/Recipe/Bourbon-Chicken/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/01/10/76/1107659.jpg", "ready_in": "", "directions": ["Place chicken breasts in a 9x13 inch baking dish. In a small bowl combine the ginger, soy sauce, onion flakes, sugar, bourbon and garlic powder. Mix together and pour mixture over chicken. Cover dish and place in refrigerator. Marinate overnight.", "Preheat oven to 325 degrees F (165 degrees C).", "Remove dish from refrigerator and remove cover. Bake in the preheated oven, basting frequently, for 1 1/2 hours or until chicken is well browned and juices run clear."], "description": "\\"A favorite named after Bourbon Street in New Orleans, Louisiana and for the bourbon whiskey ingredient; although this dish, sold as Cajun-style cuisine in malls all over America, is reported to be nothing of the kind (not genuine Creole or Cajun fare)! Note: If you double the recipe, make sure that the chicken is still in a single layer. Laissez les bons temps rouler!\\""}', '{"name": "Garlic Shrimp Linguine", "ingredients": [{"item": "uncooked linguine", "amount": "1 pound"}, {"item": "butter", "amount": "1 tablespoon"}, {"item": "white wine", "amount": "3 tablespoons"}, {"item": "grated Parmesan cheese", "amount": "2 teaspoons"}, {"item": "garlic, minced", "amount": "3 cloves"}, {"item": "chopped fresh parsley", "amount": "1 teaspoon"}, {"item": "salt and pepper to taste", "amount": "1 pinch"}, {"item": "medium shrimp, peeled and deveined", "amount": "1 pound"}], "url": "http://allrecipes.com/Recipe/Garlic-Shrimp-Linguine/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/40/34/403446.jpg", "ready_in": "30  mins", "directions": ["Bring a large pot of lightly salted water to a boil. Add pasta and cook for 8 to 10 minutes or until al dente; drain.", "In a medium saucepan, melt butter over medium low heat; add wine, cheese, garlic, parsley and salt and pepper to taste. Simmer over low heat for 3 to 5 minutes, stirring frequently.", "Increase heat to medium high and add shrimp to saucepan; cook for about 3 to 4 minutes or until shrimp begins to turn pink. Do not overcook.", "Divide pasta into portions and spoon sauce on top; garnish with Parmesan cheese and fresh parsley, if desired."], "description": "\\"A dazzlingly simple and delicious recipe. This is an elegant dish to   serve drop-in dinner guests.\\""}', '{"name": "Turkey Veggie Meatloaf Cups", "ingredients": [{"item": "coarsely chopped zucchini", "amount": "2 cups"}, {"item": "coarsely chopped onions", "amount": "1 1/2 cups"}, {"item": "red bell pepper, coarsely chopped", "amount": "1"}, {"item": "extra lean ground turkey", "amount": "1 pound"}, {"item": "uncooked couscous", "amount": "1/2 cup"}, {"item": "egg", "amount": "1"}, {"item": "Worcestershire sauce", "amount": "2 tablespoons"}, {"item": "Dijon mustard", "amount": "1 tablespoon"}, {"item": "barbecue sauce, or as needed", "amount": "1/2 cup"}], "url": "http://allrecipes.com/Recipe/Turkey-Veggie-Meatloaf-Cups/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/65/86/658635.jpg", "ready_in": "50  mins", "directions": ["Preheat oven to 400 degrees F (200 degrees C). Spray 20 muffin cups with cooking spray.", "Place zucchini, onions, and red bell pepper into a food processor, and pulse several times until finely chopped but not liquefied. Place the vegetables into a bowl, and mix in ground turkey, couscous, egg, Worcestershire sauce, and Dijon mustard until thoroughly combined. Fill each prepared muffin cup about 3/4 full. Top each cup with about 1 teaspoon of barbecue sauce.", "Bake in the preheated oven until juices run clear, about 25 minutes. Internal temperature of a muffin measured by an instant-read meat thermometer should be at least 160 degrees F (70 degrees C). Let stand 5 minutes before serving."], "description": "\\"This super-delicious and incredibly easy recipe has become one of my absolute favorites. I pass it on to everyone I know, and they all rave about it too!\\""}', '{"name": "Spicy Slow Cooker Black Bean Soup", "ingredients": [{"item": "dry black beans, soaked overnight", "amount": "1 pound"}, {"item": "diced jalapeno peppers", "amount": "4 teaspoons"}, {"item": "chicken broth", "amount": "6 cups"}, {"item": "garlic powder", "amount": "1/2 teaspoon"}, {"item": "chili powder", "amount": "1 tablespoon"}, {"item": "ground cumin", "amount": "1 teaspoon"}, {"item": "cayenne pepper", "amount": "1 teaspoon"}, {"item": "ground black pepper", "amount": "3/4 teaspoon"}, {"item": "hot pepper sauce", "amount": "1/2 teaspoon"}], "url": "http://allrecipes.com/Recipe/Spicy-Slow-Cooker-Black-Bean-Soup/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/24/05/240569.jpg", "ready_in": "6+  hrs", "directions": ["Drain black beans, and rinse.", "Combine beans, jalapenos, and chicken broth in a slow cooker. Season with garlic powder, chili powder, cumin, cayenne, pepper, and hot pepper sauce.", "Cook on High for 4 hours. Reduce heat to Low, and continue cooking for 2 hours, or until you are ready to eat."], "description": "\\"This soup can be served as is or run through the blender for a creamy version. You can adjust the spice to cool it down.\\""}', '{"name": "Black-Eyed Pea Gumbo", "ingredients": [{"item": "olive oil", "amount": "1 tablespoon"}, {"item": "onion, chopped", "amount": "1 medium"}, {"item": "green bell pepper, chopped", "amount": "1 medium"}, {"item": "celery, chopped", "amount": "5 stalks"}, {"item": "chicken broth", "amount": "2 cups"}, {"item": "brown rice", "amount": "1 cup"}, {"item": "black-eyed peas with liquid", "amount": "4 (15 ounce) cans"}, {"item": "diced tomatoes and green chiles", "amount": "1 (10 ounce) can"}, {"item": "diced tomatoes", "amount": "1 (14.5 ounce) can"}, {"item": "garlic, finely chopped", "amount": "2 cloves"}], "url": "http://allrecipes.com/Recipe/Black-Eyed-Pea-Gumbo/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/16/09/160914.jpg", "ready_in": "1\\u00bc  hrs", "directions": ["Heat the olive oil in a large saucepan over medium heat, and cook the onion, pepper, and celery until tender. Pour in the chicken broth, and mix in rice, black-eyed peas with liquid, diced tomatoes and green chiles, diced tomatoes, and garlic. Bring to a boil, reduce heat to low, and simmer 45 minutes, or until rice is tender. Add water if soup is too thick."], "description": "\\"A winter time favorite at our house. We always have it for New Year\'s Day while we watch football! Serve with a tossed salad and corn bread.\\""}', '{"name": "Maple-Garlic Marinated Pork Tenderloin", "ingredients": [{"item": "Dijon mustard", "amount": "2 tablespoons"}, {"item": "sesame oil", "amount": "1 teaspoon"}, {"item": "garlic, minced", "amount": "3 cloves"}, {"item": "fresh ground black pepper to taste", "amount": ""}, {"item": "maple syrup", "amount": "1 cup"}, {"item": "pork tenderloin", "amount": "1 1/2 pounds"}], "url": "http://allrecipes.com/Recipe/Maple-Garlic-Marinated-Pork-Tenderloin/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/21/66/216630.jpg", "ready_in": "8+  hrs", "directions": ["Combine mustard, sesame oil, garlic, pepper, and maple syrup. Place pork in a shallow dish and coat thoroughly with marinade. Cover, then chill in the refrigerator at least eight hours, or overnight.", "Preheat grill for medium-low heat.", "Remove pork from marinade, and set aside. Transfer remaining marinade to a small saucepan, and cook on the stove over medium-low heat for 5 minutes.", "Brush grate with oil, and place meat on grate. Grill pork, basting with reserved marinade, for approximately 15 to 25 minutes, or until interior is no longer pink. Avoid using high temperatures as marinade will burn."], "description": "\\"A delicious, sweet marinade using real maple syrup. Great on chicken or pork tenderloin.\\""}', '{"name": "Jenny\'s Grilled Chicken Breasts", "ingredients": [{"item": "skinless, boneless chicken breast halves", "amount": "4 skinless, boneless chicken breast halves"}, {"item": "lemon juice", "amount": "1/2 cup"}, {"item": "onion powder", "amount": "1/2 teaspoon"}, {"item": "ground black pepper to taste", "amount": ""}, {"item": "seasoning salt to taste", "amount": ""}, {"item": "dried parsley", "amount": "2 teaspoons"}], "url": "http://allrecipes.com/Recipe/Jennys-Grilled-Chicken-Breasts/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/23/74/237426.jpg", "ready_in": "45  mins", "directions": ["Preheat an outdoor grill for medium-high heat, and lightly oil grate.", "Dip chicken in lemon juice, and sprinkle with the onion powder, ground black pepper, seasoning salt and parsley. Discard any remaining lemon juice.", "Cook on the prepared grill 10 to 15 minutes per side, or until no longer pink and juices run clear."], "description": "\\"This is the recipe that my friends and family still beg me to make when the grill is brought out. It\'s so easy and versatile, and can be tried on several different meats. I like it with scalloped potatoes, baked potatoes or rice pilaf. Try it with cilantro or oregano instead of parsley. Save leftovers for salad the next day.\\""}', '{"name": "Split Pea and Ham Soup I", "ingredients": [{"item": "chopped onion", "amount": "1 cup"}, {"item": "vegetable oil", "amount": "1 teaspoon"}, {"item": "dried split peas", "amount": "1 pound"}, {"item": "ham bone", "amount": "1 pound"}, {"item": "salt and pepper to taste", "amount": "1 pinch"}], "url": "http://allrecipes.com/Recipe/Split-Pea-and-Ham-Soup-I/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/02/83/28341.jpg", "ready_in": "2\\u00bd  hrs", "directions": ["In a medium pot, saute onions in oil.  Add the split peas, ham bone, and enough water to cover ingredients; season with salt and pepper.", "Cover, and cook until there are no peas left, just a green liquid, 2 hours.  While it is cooking, check to see if water has evaporated. You may need to add more water as the soup continues to cook.", "Once the soup is a green liquid remove from heat, and let stand so it will thicken. Once thickened you may need to heat through to serve."], "description": "\\"This is a good way to use leftover ham, quite inexpensive, and VERY tasty.  I hope that you enjoy it.  Some people like to add carrots or other types of vegetables.  Don\'t forget to serve with buttered bread toasted in the oven.\\""}', '{"name": "Pasta Fagioli", "ingredients": [{"item": "olive oil", "amount": "1 tablespoon"}, {"item": "celery, chopped", "amount": "2 stalks"}, {"item": "onion, chopped", "amount": "1"}, {"item": "garlic, minced", "amount": "3 cloves"}, {"item": "dried parsley", "amount": "2 teaspoons"}, {"item": "Italian seasoning", "amount": "1 teaspoon"}, {"item": "crushed red pepper flakes", "amount": "1/4 teaspoon"}, {"item": "salt to taste", "amount": ""}, {"item": "chicken broth", "amount": "1 (14.5 ounce) can"}, {"item": "medium tomatoes, peeled and chopped", "amount": "2"}, {"item": "tomato sauce", "amount": "1 (8 ounce) can"}, {"item": "uncooked spinach pasta", "amount": "1/2 cup"}, {"item": "cannellini beans, with liquid", "amount": "1 (15 ounce) can"}], "url": "http://allrecipes.com/Recipe/Pasta-Fagioli/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/70/23/702325.jpg", "ready_in": "50  mins", "directions": ["Heat olive oil in a large saucepan over medium heat. Cook celery, onion, garlic, parsley, Italian seasoning, red pepper flakes, and salt in the hot oil until onion is translucent, about 5 minutes. Stir in chicken broth, tomatoes and tomato sauce, and simmer on low for 15 to 20 minutes.", "Add pasta and cook 10 minutes, until pasta is tender.", "Add undrained beans and mix well. Heat through. Serve with grated Parmesan cheese sprinkled on top."], "description": "\\"A traditional Italian soup. Serve with a crisp salad and a hot loaf of garlic bread and you have a meal! Serve with grated Parmesan cheese on top.\\""}', '{"name": "Easy Grilled Chicken Teriyaki", "ingredients": [{"item": "skinless, boneless chicken breast halves", "amount": "4"}, {"item": "teriyaki sauce", "amount": "1 cup"}, {"item": "lemon juice", "amount": "1/4 cup"}, {"item": "minced fresh garlic", "amount": "2 teaspoons"}, {"item": "sesame oil", "amount": "2 teaspoons"}], "url": "http://allrecipes.com/Recipe/Easy-Grilled-Chicken-Teriyaki/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/01/58/15810.jpg", "ready_in": "1+  days", "directions": ["Place chicken, teriyaki sauce, lemon juice, garlic, and sesame oil in a large resealable plastic bag. Seal bag, and shake to coat. Place in refrigerator for 24 hours, turning every so often.", "Preheat grill for high heat.", "Lightly oil the grill grate. Remove chicken from bag, discarding any remaining marinade. Grill for 6 to 8 minutes each side, or until juices run clear when chicken is pierced with a fork."], "description": "\\"Chicken breasts marinated in teriyaki sauce, lemon, garlic, and sesame oil, then grilled to a tasty finish. Very easy and great for a hot summer\'s evening. Leftovers are great on a green salad or sandwich. Be sure to grill very hot and very fast!!\\""}', '{"name": "A Jerky Chicken", "ingredients": [{"item": "onion, finely chopped", "amount": "1 teaspoon"}, {"item": "brown sugar", "amount": "3 tablespoons"}, {"item": "soy sauce", "amount": "4 tablespoons"}, {"item": "red wine vinegar", "amount": "4 tablespoons"}, {"item": "chopped fresh thyme", "amount": "2 teaspoons"}, {"item": "sesame oil", "amount": "1 teaspoon"}, {"item": "garlic, chopped", "amount": "3 cloves"}, {"item": "ground allspice", "amount": "1/2 teaspoon"}, {"item": "habanero pepper, sliced", "amount": "1"}, {"item": "skinless, boneless chicken breast halves - cut into 1 inch strips", "amount": "4"}], "url": "http://allrecipes.com/Recipe/A-Jerky-Chicken/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/22/38/223805.jpg", "ready_in": "1\\u00bc  hrs", "directions": ["Combine the onion, brown sugar, soy sauce, vinegar, thyme, sesame oil, garlic, allspice and habanero pepper in the container of a food processor or blender. Process until smooth. Place the chicken into a large resealable bag, and pour in 3/4 of the sauce. Squeeze out excess air, and seal. Marinate in the refrigerator for at least one hour.", "Preheat your oven\'s broiler.", "Remove chicken from bag, and discard marinade. Broil chicken for 10 to 15 minutes, turning once to ensure even cooking. Heat remaining sauce in a small pan, and pour over chicken when serving."], "description": "\\"A great Jamaican style dish. A spicy blend of herbs, vinegar, and a habanero pepper make this chicken dish unforgettable. Serve with rice, yum!\\""}', '{"name": "Brazilian Black Bean Stew", "ingredients": [{"item": "canola oil", "amount": "1 tablespoon"}, {"item": "chorizo sausage, chopped", "amount": "1/4 pound"}, {"item": "cooked ham, chopped", "amount": "1/3 pound"}, {"item": "onion, chopped", "amount": "1 medium"}, {"item": "garlic, minced", "amount": "2 cloves"}, {"item": "sweet potatoes, peeled and diced", "amount": "2 (1 pound)"}, {"item": "red bell pepper, diced", "amount": "1 large"}, {"item": "diced tomatoes with juice", "amount": "2 (14.5 ounce) cans"}, {"item": "small hot green chile pepper, diced", "amount": "1"}, {"item": "water", "amount": "1 1/2 cups"}, {"item": "black beans, rinsed and drained", "amount": "2 (16 ounce) cans"}, {"item": "mango - peeled, seeded and diced", "amount": "1"}, {"item": "chopped fresh cilantro", "amount": "1/4 cup"}, {"item": "salt", "amount": "1/4 teaspoon"}], "url": "http://allrecipes.com/Recipe/Brazilian-Black-Bean-Stew/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/05/63/56386.jpg", "ready_in": "45  mins", "directions": ["Heat the oil in a large pot over medium heat, and cook the chorizo and ham 2 to 3 minutes. Place the onion in the pot, and cook until tender. Stir in garlic, and cook until tender, then mix in the sweet potatoes, bell pepper, tomatoes with juice, chile pepper, and water. Bring to a boil, reduce heat to low, cover, and simmer 15 minutes, until sweet potatoes are tender.", "Stir the beans into the pot, and cook uncovered until heated through. Mix in the mango and cilantro, and season with salt."], "description": "\\"This easy yet wonderful stew can be made any time of year. The meat can be omitted without compromising the dish. Serve hot with a great bread and a nice spinach salad.\\""}', '{"name": "Bean Quesadillas", "ingredients": [{"item": "vegetable oil", "amount": "1 tablespoon"}, {"item": "onion, finely diced", "amount": "1"}, {"item": "garlic, minced", "amount": "2 cloves"}, {"item": "black beans, rinsed and drained", "amount": "1 (15 ounce) can"}, {"item": "green bell pepper, chopped", "amount": "1"}, {"item": "tomatoes, chopped", "amount": "2"}, {"item": "frozen corn", "amount": "1/2 (10 ounce) package"}, {"item": "flour tortillas", "amount": "12 (12 inch)"}, {"item": "shredded Cheddar cheese", "amount": "1 cup"}, {"item": "vegetable oil", "amount": "1/4 cup"}], "url": "http://allrecipes.com/Recipe/Bean-Quesadillas/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/25/48/254852.jpg", "ready_in": "45  mins", "directions": ["Heat 1 tablespoon oil in a skillet over medium heat, and saute the onion and garlic until soft. Mix in beans, bell pepper, tomatoes, and corn; cook until heated through.", "Spread 6 tortillas with equal amounts of the bean and vegetable mixture. Sprinkle with equal amounts of the Cheddar cheese, and top with the remaining tortillas to form quesadillas.", "Heat 1/4 cup oil in a large skillet over medium-high heat. Place quesadillas in the skillet and cook, turning once, until cheese is melted and both sides are lightly browned."], "description": "\\"Easy and yummy! Veggies, beans, and cheese sandwiched in tortillas. You could add or substitute any vegetables that you\'d like. Serve with sour cream and rice.\\""}', '{"name": "Baked Halibut Steaks", "ingredients": [{"item": "olive oil", "amount": "1 teaspoon"}, {"item": "diced zucchini", "amount": "1 cup"}, {"item": "minced onion", "amount": "1/2 cup"}, {"item": "garlic, peeled and minced", "amount": "1 clove"}, {"item": "diced fresh tomatoes", "amount": "2 cups"}, {"item": "chopped fresh basil", "amount": "2 tablespoons"}, {"item": "salt", "amount": "1/4 teaspoon"}, {"item": "ground black pepper", "amount": "1/4 teaspoon"}, {"item": "halibut steaks", "amount": "4 (6 ounce)"}, {"item": "crumbled feta cheese", "amount": "1/3 cup"}], "url": "http://allrecipes.com/Recipe/Baked-Halibut-Steaks/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/40/36/403623.jpg", "ready_in": "30  mins", "directions": ["Preheat oven to 450 degrees F (230 degrees C). Lightly grease a shallow baking dish.", "Heat olive oil in a medium saucepan over medium heat and stir in zucchini, onion, and garlic. Cook and stir 5 minutes or until tender. Remove saucepan from heat and mix in tomatoes, basil, salt, and pepper.", "Arrange halibut steaks in a single layer in the prepared baking dish. Spoon equal amounts of the zucchini mixture over each steak. Top with feta cheese.", "Bake 15 minutes in the preheated oven, or until fish is easily flaked with a fork."], "description": "\\"An Italian-style vegetable and feta cheese topping is the perfect enhancement to delicious baked halibut.\\""}', '{"name": "Slow Cooker Honey Garlic Chicken", "ingredients": [{"item": "vegetable oil", "amount": "1 tablespoon"}, {"item": "boneless, skinless chicken thighs", "amount": "10"}, {"item": "honey", "amount": "3/4 cup"}, {"item": "lite soy sauce", "amount": "3/4 cup"}, {"item": "ketchup", "amount": "3 tablespoons"}, {"item": "garlic, crushed", "amount": "2 cloves"}, {"item": "minced fresh ginger root", "amount": "1 tablespoon"}, {"item": "pineapple tidbits, drained with juice reserved", "amount": "1 (20 ounce) can"}, {"item": "cornstarch", "amount": "2 tablespoons"}, {"item": "water", "amount": "1/4 cup"}], "url": "http://allrecipes.com/Recipe/Slow-Cooker-Honey-Garlic-Chicken/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/08/64/86439.jpg", "ready_in": "4+  hrs", "directions": ["Heat oil in a skillet over medium heat, and cook chicken thighs just until evenly browned on all sides. Place thighs in a slow cooker.", "In a bowl, mix honey, soy sauce, ketchup, garlic, ginger, and reserved pineapple juice. Pour into the slow cooker.", "Cover, and cook 4 hours on High. Stir in pineapple tidbits just before serving.", "Mix the cornstarch and water in a small bowl. Remove thighs from slow cooker. Blend the cornstarch mixture into remaining sauce in the slow cooker to thicken. Serve sauce over the chicken."], "description": "\\"Easy to prepare and very tasty. Can also be baked with the same results if you don\'t have a slow cooker. Serve over rice.\\""}', '{"name": "Asian Tuna Patties", "ingredients": [{"item": "tuna, drained and flaked", "amount": "2 (6 ounce) cans"}, {"item": "egg, beaten", "amount": "1"}, {"item": "dry bread crumbs", "amount": "3/4 cup"}, {"item": "green onions, minced", "amount": "3"}, {"item": "garlic, peeled and minced", "amount": "1 clove"}, {"item": "soy sauce", "amount": "1 tablespoon"}, {"item": "teriyaki sauce", "amount": "1 tablespoon"}, {"item": "ketchup", "amount": "1 tablespoon"}, {"item": "sesame oil", "amount": "1 teaspoon"}, {"item": "black pepper", "amount": "1 teaspoon"}, {"item": "cornmeal", "amount": "1/2 cup"}, {"item": "vegetable oil", "amount": "2 tablespoons"}], "url": "http://allrecipes.com/Recipe/Asian-Tuna-Patties/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/13/04/130471.jpg", "ready_in": "35  mins", "directions": ["In a large bowl, mix tuna, egg, bread crumbs, green onions, and garlic. Blend soy sauce, teriyaki sauce, ketchup, sesame oil, and pepper into the mixture. Form the mixture into about 6 patties approximately 1 inch thick. Lightly sprinkle each patty on all sides with cornmeal.", "Heat oil in a medium skillet over medium heat. Fry each patty about 5 minutes on each side, until golden brown."], "description": "\\"A delicious, fast, and different way to eat tuna. Everyone who has tried this has asked for the recipe.\\""}', '{"name": "Amatriciana", "ingredients": [{"item": "bacon, diced", "amount": "4 slices"}, {"item": "chopped onion", "amount": "1/2 cup"}, {"item": "minced garlic", "amount": "1 teaspoon"}, {"item": "crushed red pepper flakes", "amount": "1/4 teaspoon"}, {"item": "stewed tomatoes", "amount": "2 (14.5 ounce) cans"}, {"item": "linguine pasta, uncooked", "amount": "1 pound"}, {"item": "chopped fresh basil", "amount": "1 tablespoon"}, {"item": "grated Parmesan cheese", "amount": "2 tablespoons"}], "url": "http://allrecipes.com/Recipe/Amatriciana/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/47/94/479412.jpg", "ready_in": "35  mins", "directions": ["Cook diced bacon in a large saucepan over medium high heat until crisp, about 5 minutes.  Drain all but 2 tablespoons of drippings from the pan.", "Add onions, and cook over medium heat about 3 minutes.  Stir in garlic and red pepper flakes; cook 30 seconds.  Add canned tomatoes, undrained; simmer 10 minutes, breaking up tomatoes.", "Meanwhile, cook the pasta in a large pot of 4 quarts boiling salted water until al dente. Drain.", "Stir basil into the sauce, and then toss with cooked pasta.  Serve with grated Parmesan cheese."], "description": "\\"This is a classic Italian pasta dish. Use fresh basil when it\'s in season; at other times, use fresh flat-leaf parsley.\\""}', '{"name": "The Ultimate Chili", "ingredients": [{"item": "lean ground beef", "amount": "1 pound"}, {"item": "salt and pepper to taste", "amount": ""}, {"item": "dark red kidney beans", "amount": "3 (15 ounce) cans"}, {"item": "Mexican-style stewed tomatoes", "amount": "3 (14.5 ounce) cans"}, {"item": "celery, chopped", "amount": "2 stalks"}, {"item": "red bell pepper, chopped", "amount": "1"}, {"item": "red wine vinegar", "amount": "1/4 cup"}, {"item": "chili powder", "amount": "2 tablespoons"}, {"item": "ground cumin", "amount": "1 teaspoon"}, {"item": "dried parsley", "amount": "1 teaspoon"}, {"item": "dried basil", "amount": "1 teaspoon"}, {"item": "Worcestershire sauce", "amount": "1 dash"}, {"item": "red wine", "amount": "1/2 cup"}], "url": "http://allrecipes.com/Recipe/The-Ultimate-Chili/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/01/00/39/1003997.jpg", "ready_in": "6+  hrs", "directions": ["In a large skillet over medium-high heat, cook ground beef until evenly browned. Drain off grease, and season to taste with salt and pepper.", "In a slow cooker, combine the cooked beef, kidney beans, tomatoes, celery, red bell pepper, and red wine vinegar. Season with chili powder, cumin, parsley, basil and Worcestershire sauce. Stir to distribute ingredients evenly.", "Cook on High for 6 hours, or on Low for 8 hours. Pour in the wine during the last 2 hours."], "description": "\\"Easy recipe with little preparation time. This can also be made with ground turkey, and it tastes even better the next day!\\""}', '{"name": "Rosemary Turkey Meatloaf", "ingredients": [{"item": "ground turkey", "amount": "1 1/2 pounds"}, {"item": "dry bread crumbs", "amount": "2 cups"}, {"item": "onion, chopped", "amount": "1"}, {"item": "egg, beaten", "amount": "1"}, {"item": "milk", "amount": "1 cup"}, {"item": "balsamic vinegar", "amount": "1/2 cup"}, {"item": "garlic, minced", "amount": "1 clove"}, {"item": "salt", "amount": "1 teaspoon"}, {"item": "pepper", "amount": "1 teaspoon"}, {"item": "chopped fresh rosemary", "amount": "1 1/2 tablespoons"}, {"item": "\\u00a0", "amount": ""}, {"item": "canned tomato sauce", "amount": "1 cup"}, {"item": "brown sugar", "amount": "3/4 cup"}, {"item": "Dijon mustard", "amount": "1 tablespoon"}], "url": "http://allrecipes.com/Recipe/Rosemary-Turkey-Meatloaf/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/01/57/15733.jpg", "ready_in": "1\\u00bc  hrs", "directions": ["Preheat oven to 350 degrees F (175 degrees C). Lightly grease a 9x5 inch loaf pan.", "In a large mixing bowl, mix together the ground turkey, bread crumbs, onion, egg and milk. Season with balsamic vinegar, salt, pepper and rosemary. Press into the prepared pan. Blend together the tomato sauce, brown sugar and mustard; pour evenly over the top of the loaf.", "Bake for 1 hour in the preheated oven, or until juices run clear when pricked with a knife."], "description": "\\"This is a delicious alternative to the usual meatloaf.\\""}', '{"name": "Ranch Crispy Chicken", "ingredients": [{"item": "skinless, boneless chicken breast halves", "amount": "8 skinless, boneless chicken breast halves"}, {"item": "ranch dressing mix", "amount": "2 (1 ounce) packages"}, {"item": "dry bread crumbs", "amount": "1/4 cup"}], "url": "http://allrecipes.com/Recipe/Ranch-Crispy-Chicken/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/49/71/497174.jpg", "ready_in": "45  mins", "directions": ["Preheat oven to 375 degrees F (190 degrees C).", "Combine dressing mix and bread crumbs in a plastic bag. Add chicken and shake until coated.", "Place coated chicken pieces on an ungreased cookie sheet and bake in preheated oven for 25 to 30 minutes, or until chicken is cooked through and juices run clear. Serve with rice or potatoes, if desired."], "description": "\\"This recipe is both delicious AND insanely easy!\\""}', '{"name": "Quinoa with Chickpeas and Tomatoes", "ingredients": [{"item": "quinoa", "amount": "1 cup"}, {"item": "salt", "amount": "1/8 teaspoon"}, {"item": "water", "amount": "1 3/4 cups"}, {"item": "canned garbanzo beans (chickpeas), drained", "amount": "1 cup"}, {"item": "tomato, chopped", "amount": "1"}, {"item": "garlic, minced", "amount": "1 clove"}, {"item": "lime juice", "amount": "3 tablespoons"}, {"item": "olive oil", "amount": "4 teaspoons"}, {"item": "ground cumin", "amount": "1/2 teaspoon"}, {"item": "salt and pepper to taste", "amount": "1 pinch"}, {"item": "chopped fresh parsley", "amount": "1/2 teaspoon"}], "url": "http://allrecipes.com/Recipe/Quinoa-with-Chickpeas-and-Tomatoes/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/01/12/81/1128177.jpg", "ready_in": "40  mins", "directions": ["Place the quinoa in a fine mesh strainer, and rinse under cold, running water until the water no longer foams. Bring the quinoa, salt, and water to a boil in a saucepan. Reduce heat to medium-low, cover, and simmer until the quinoa is tender, 20 to 25 minutes.", "Once done, stir in the garbanzo beans, tomatoes, garlic, lime juice, and olive oil. Season with cumin, salt, and pepper. Sprinkle with chopped fresh parsley to serve."], "description": "\\"This delicious recipe was presented to me by a vegan friend.  The lime juice gives the quinoa a fresh flavor that can\'t be beat!\\""}', '{"name": "Pork Chops in Beer", "ingredients": [{"item": "ketchup", "amount": "2 cups"}, {"item": "beer", "amount": "1 (12 fluid ounce) can or bottle"}, {"item": "packed brown sugar", "amount": "3/4 cup"}, {"item": "pork chops", "amount": "8"}], "url": "http://allrecipes.com/Recipe/Pork-Chops-in-Beer/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/80/45/804578.jpg", "ready_in": "1\\u00bd  hrs", "directions": ["Preheat oven to 350 degrees F (175 degrees C).", "In a medium bowl, combine the ketchup, brown sugar and beer. Mix well and pour into a 9x13 inch baking dish. Place the pork chops over this mixture in the dish.", "Bake, uncovered, at 350 degrees F (175 degrees C) for 1 hour, or internal pork temperature reaches 145 degrees F (63 degrees C). (Note: Place foil over pork chops if they start to brown too quickly.)"], "description": "\\"This is an easy pork chop recipe using just beer, ketchup and brown sugar. It\'s simple and quite good.\\""}', '{"name": "Tangy Grilled Pork Tenderloin", "ingredients": [{"item": "pork tenderloin", "amount": "2 pounds"}, {"item": "honey", "amount": "2/3 cup"}, {"item": "Dijon mustard", "amount": "1/2 cup"}, {"item": "chili powder", "amount": "1/4 teaspoon"}, {"item": "salt", "amount": "1/4 teaspoon"}], "url": "http://allrecipes.com/Recipe/Tangy-Grilled-Pork-Tenderloin/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/72/06/720645.jpg", "ready_in": "4+  hrs", "directions": ["Place meat in a large resealable plastic bag. In a medium bowl, mix together honey, Dijon mustard, chili powder, and salt. Pour marinade over tenderloins, seal, and refrigerate for at least 4 hours.", "Prepare the grill for indirect heat.", "Lightly oil grill grate. Remove meat from marinade, and discard liquid. Grill for 15 to 25 minutes, or until an instant-read thermometer inserted into the center reads 145 degrees F (63 degrees C)."], "description": "\\"This is a quick, easy and very flavorful recipe. I\'ve made it many times. Plan ahead because the pork needs to marinate.\\""}', '{"name": "Easy White Chili", "ingredients": [{"item": "olive oil", "amount": "2 tablespoons"}, {"item": "onions, chopped", "amount": "2"}, {"item": "garlic, minced", "amount": "4 cloves"}, {"item": "cooked, boneless chicken breast half, chopped", "amount": "4"}, {"item": "chicken broth", "amount": "3 (14.5 ounce) cans"}, {"item": "canned green chile peppers, chopped", "amount": "2 (4 ounce) cans"}, {"item": "ground cumin", "amount": "2 teaspoons"}, {"item": "dried oregano", "amount": "2 teaspoons"}, {"item": "cayenne pepper", "amount": "1 1/2 teaspoons"}, {"item": "great Northern beans, undrained", "amount": "5 (14.5 ounce) cans"}, {"item": "shredded Monterey Jack cheese", "amount": "1 cup"}], "url": "http://allrecipes.com/Recipe/Easy-White-Chili/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/56/17/561743.jpg", "ready_in": "45  mins", "directions": ["Heat the oil in a large pot over medium heat. Add the onions and garlic and saute for 10 minutes, or until onions are tender. Add the chicken, chicken broth, green chile peppers, cumin, oregano and cayenne pepper and bring to a boil.", "Reduce heat to low and add the beans. Simmer for 20 to 30 minutes, or until heated thoroughly. Pour into individual bowls and top with the cheese."], "description": "\\"A very tasty, somewhat spicy, variation of chili that uses chicken. I like to serve it with sweet corn bread.\\""}', '{"name": "Grilled Peanut Chicken", "ingredients": [{"item": "reduced fat peanut butter", "amount": "2 tablespoons"}, {"item": "fresh lime juice", "amount": "1 tablespoon"}, {"item": "soy sauce", "amount": "2 teaspoons"}, {"item": "garlic, chopped", "amount": "1 clove"}, {"item": "curry powder", "amount": "1/3 teaspoon"}, {"item": "ground cayenne pepper", "amount": "1 dash"}, {"item": "skinless, boneless chicken breast halves", "amount": "4 skinless, boneless chicken breast halves"}], "url": "http://allrecipes.com/Recipe/Grilled-Peanut-Chicken/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/03/91/39170.jpg", "ready_in": "30  mins", "directions": ["Preheat grill for high heat.", "In a bowl, mix the peanut butter, lime juice, soy sauce, garlic, curry powder, and cayenne pepper.", "Lightly oil the grill grate. Place chicken on grate, and brush with 1/2 the sauce. Grill 6 to 8 minutes. Turn chicken, and brush with remaining sauce. Continue grilling 6 to 8 minutes, until chicken juices run clear."], "description": "\\"This is a perfect dish when you have company coming and no idea what to fix them, especially when you don\'t want to be boring about dinner.\\""}', '{"name": "Baked Chicken with Peaches", "ingredients": [{"item": "skinless, boneless chicken breast halves", "amount": "8"}, {"item": "brown sugar", "amount": "1 cup"}, {"item": "fresh peaches - peeled, pitted and sliced", "amount": "4"}, {"item": "ground ginger", "amount": "1/8 teaspoon"}, {"item": "ground cloves", "amount": "1/8 teaspoon"}, {"item": "fresh lemon juice", "amount": "2 tablespoons"}], "url": "http://allrecipes.com/Recipe/Baked-Chicken-with-Peaches/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/11/65/116547.jpg", "ready_in": "45  mins", "directions": ["Preheat oven to 350 degrees F (175 degrees C). Lightly grease a 9x13 inch baking dish.", "Place chicken in the prepared baking dish, and sprinkle with 1/2 cup of brown sugar. Place peach slices over chicken, then sprinkle with remaining 1/2 cup brown sugar, ginger, cloves, and lemon juice.", "Bake for about 30 minutes in the preheated oven, basting often with juices, until chicken is cooked through and juices run clear."], "description": "\\"Rushed? Need an elegant main dish to serve for an important occasion, that doesn\'t take a lot of preparation or time? This is it. This dish is not only easy and quick, it serves up beautifully as well. My family loves it. Tip: For a delicious sauce to serve over the chicken and peaches, mix 1/2 tablespoon cornstarch with 1/2 cup cold water and pour mixture into cooking juices. This mixture will thicken and become a sauce to pour over the chicken and peaches.\\""}', '{"name": "Chicken in a Pot", "ingredients": [{"item": "chicken broth", "amount": "3/4 cup"}, {"item": "tomato paste", "amount": "1 1/2 tablespoons"}, {"item": "ground black pepper", "amount": "1/4 teaspoon"}, {"item": "dried oregano", "amount": "1/2 teaspoon"}, {"item": "salt", "amount": "1/8 teaspoon"}, {"item": "garlic, minced", "amount": "1 clove"}, {"item": "boneless, skinless chicken breast halves", "amount": "4"}, {"item": "dry bread crumbs", "amount": "3 tablespoons"}, {"item": "olive oil", "amount": "2 teaspoons"}, {"item": "fresh sliced mushrooms", "amount": "2 cups"}], "url": "http://allrecipes.com/Recipe/Chicken-in-a-Pot/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/04/97/49747.jpg", "ready_in": "40  mins", "directions": ["In a medium bowl, combine the broth, tomato paste, ground black pepper, oregano, salt and garlic. Mix well and set aside.", "Dredge the chicken in the bread crumbs, coating well. Heat the oil in a large skillet over medium high heat. Saute the chicken in the oil for 2 minutes per side, or until lightly browned.", "Add the reserved broth mixture and the mushrooms to the skillet and bring to a boil. Then cover, reduce heat to low and simmer for 20 minutes. Remove chicken and set aside, covering to keep it warm.", "Bring broth mixture to a boil and cook for 4 minutes, or until reduced to desired thickness. Spoon sauce over the chicken and serve."], "description": "\\"This dish uses just one skillet to prepare. Quick, easy and delicious. Tomato paste and chicken broth combine to make a tasty sauce. Garnish with fresh parsley.\\""}', '{"name": "Chicken and Sausage with Bowties", "ingredients": [{"item": "uncooked farfalle pasta", "amount": "1 (16 ounce) package"}, {"item": "skinless, boneless chicken breasts", "amount": "2"}, {"item": "hot Italian turkey sausage, casings removed", "amount": "1 pound"}, {"item": "olive oil", "amount": "1 tablespoon"}, {"item": "garlic, sliced", "amount": "2 cloves"}, {"item": "crushed tomatoes", "amount": "1 (14.5 ounce) can"}, {"item": "red wine", "amount": "1/2 cup"}, {"item": "chopped fresh basil", "amount": "2 tablespoons"}, {"item": "dried rosemary", "amount": "1 teaspoon"}], "url": "http://allrecipes.com/Recipe/Chicken-and-Sausage-with-Bowties/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/01/08/49/1084963.jpg", "ready_in": "1  hr", "directions": ["Bring a large pot of lightly salted water to a boil. Add pasta and cook for 8 to 10 minutes or until al dente; drain.", "Wash chicken breasts and cut into large bite-size pieces. Cut sausages into large pieces. In a large, deep skillet over medium low heat, combine oil and garlic; and cook just enough to flavor the oil. Remove garlic from oil.", "Add chicken and sausage to skillet and brown both lightly until they are opaque. Add the tomatoes and the wine. Bring to a boil and let simmer for 20 minutes. Season sauce mixture with basil, rosemary and salt and pepper to taste. Add cooked, drained pasta to the skillet mixture. Toss and serve."], "description": "\\"Necessity is the mother of invention. This recipe was created by me because I had a 1 1/2 hour commute to/from work. I have two teenage boys who require a hearty meal to sustain them for at least 1 hour (maybe less). This recipe can be varied; use pork italian sausage if you like (cook longer), veal or pork cutlets or cubes (be careful to cook pork products thoroughly). Serve as a sandwich, skip the pasta, mix with leftover cooked rice; add small green peas or broccoli. I doubt if anything you try will go wrong. Mangia!\\""}', '{"name": "RamJam Chicken", "ingredients": [{"item": "soy sauce", "amount": "1/4 cup"}, {"item": "dry white wine", "amount": "3 tablespoons"}, {"item": "lemon juice", "amount": "2 tablespoons"}, {"item": "vegetable oil", "amount": "2 tablespoons"}, {"item": "dried Italian-style seasoning", "amount": "3/4 teaspoon"}, {"item": "grated fresh ginger root", "amount": "1 teaspoon"}, {"item": "garlic, crushed", "amount": "1 clove"}, {"item": "onion powder", "amount": "1/4 teaspoon"}, {"item": "ground black pepper", "amount": "1 pinch"}, {"item": "skinless, boneless chicken breast halves - cut into strips", "amount": "8"}], "url": "http://allrecipes.com/Recipe/RamJam-Chicken/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/01/01/24/1012406.jpg", "ready_in": "3+  hrs", "directions": ["In a large, resealable plastic bag, combine the soy sauce, wine, lemon juice, oil, Italian-style seasoning, ginger, garlic, onion powder, and ground black pepper. Place chicken in the bag. Seal, and let marinate in the refrigerator for at least 3 hours, or overnight.", "Preheat an outdoor grill for medium-high heat.", "Thread the chicken onto skewers, and set aside. Pour marinade into a small saucepan, and bring to a boil over high heat.", "Lightly oil the grill grate. Cook chicken on the prepared grill for approximately 8 minutes per side, basting with the sauce several times. Chicken is done when juices run clear."], "description": "\\"This is my absolute favorite marinade for chicken. I could eat this every night! The longer you let it marinate, the more intense the flavor. I usually let it sit overnight in the refrigerator, but a few hours will do.\\""}', '{"name": "Cajun Pasta Fresca", "ingredients": [{"item": "vermicelli pasta", "amount": "1 pound"}, {"item": "olive oil", "amount": "2 tablespoons"}, {"item": "minced garlic", "amount": "1 teaspoon"}, {"item": "roma (plum) tomatoes, chopped", "amount": "13"}, {"item": "salt", "amount": "1 tablespoon"}, {"item": "chopped fresh parsley", "amount": "1 tablespoon"}, {"item": "Cajun seasoning", "amount": "1 tablespoon"}, {"item": "shredded mozzarella cheese", "amount": "1/2 cup"}, {"item": "grated Parmesan cheese", "amount": "1/2 cup"}], "url": "http://allrecipes.com/Recipe/Cajun-Pasta-Fresca/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/04/01/40149.jpg", "ready_in": "25  mins", "directions": ["Bring a large pot of lightly salted water to a boil. Add pasta and cook for 8 to 10 minutes or until al dente; drain.", "While the pasta water is boiling, in a large skillet over medium heat, briefly saute garlic in oil.  Stir in tomatoes and their juice and sprinkle with salt.  When tomatoes are bubbly, mash slightly with a fork.  Stir in parsley, reduce heat and simmer 5 minutes more.", "Toss hot pasta with tomato sauce, Cajun seasoning, mozzarella and Parmesan."], "description": "\\"This is my absolute favorite dish! If you like food with just a little kick and you are a pasta fan...this recipe is for you!\\""}', '{"name": "Greek Lentil Soup (Fakes)", "ingredients": [{"item": "brown lentils", "amount": "8 ounces"}, {"item": "olive oil", "amount": "1/4 cup"}, {"item": "minced garlic", "amount": "1 tablespoon"}, {"item": "onion, minced", "amount": "1"}, {"item": "carrot, chopped", "amount": "1 large"}, {"item": "water", "amount": "1 quart"}, {"item": "dried oregano", "amount": "1 pinch"}, {"item": "crushed dried rosemary", "amount": "1 pinch"}, {"item": "bay leaves", "amount": "2"}, {"item": "tomato paste", "amount": "1 tablespoon"}, {"item": "salt and ground black pepper to taste", "amount": ""}, {"item": "olive oil, or to taste", "amount": "1 teaspoon"}, {"item": "red wine vinegar, or to taste", "amount": "1 teaspoon"}], "url": "http://allrecipes.com/Recipe/Greek-Lentil-Soup-Fakes/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/35/77/357743.jpg", "ready_in": "1\\u00bd  hrs", "directions": ["Place lentils in a large saucepan; add enough water to cover by 1 inch. Bring water to a boil and cook until tender, about 10 minutes; drain.", "Heat olive oil in a saucepan over medium heat. Add garlic, onion, and carrot; cook and stir until the onion has softened and turned translucent, about 5 minutes. Pour in lentils, 1 quart water, oregano, rosemary, and bay leaves. Bring to a boil. Reduce heat to medium-low, cover, and simmer for 10 minutes.", "Stir in tomato paste and season with salt and pepper. Cover and simmer until the lentils have softened, 30 to 40 minutes, stirring occasionally. Add additional water if the soup becomes too thick. Drizzle with 1 teaspoon olive oil and red wine vinegar to taste."], "description": "\\"Fakes (pronounced \'Fah-kehs\') is a staple in the Greek kitchen, and an especially filling meal. It is traditionally served with a drizzle of olive oil and lots of vinegar. Though the vinegar is of course optional, try it. It lifts the lentils and adds another dimension of flavor!\\""}', '{"name": "Navy Bean Soup I", "ingredients": [{"item": "dried navy beans", "amount": "1 (16 ounce) package"}, {"item": "water", "amount": "6 cups"}, {"item": "diced tomatoes", "amount": "1 (14.5 ounce) can"}, {"item": "onion, chopped", "amount": "1"}, {"item": "celery, chopped", "amount": "2 stalks"}, {"item": "garlic, minced", "amount": "1 clove"}, {"item": "chopped ham", "amount": "1/2 pound"}, {"item": "chicken bouillon", "amount": "1 cube"}, {"item": "Worcestershire sauce", "amount": "2 tablespoons"}, {"item": "dried parsley", "amount": "1 tablespoon"}, {"item": "garlic powder", "amount": "2 teaspoons"}, {"item": "bay leaf", "amount": "1"}, {"item": "salt", "amount": "1 teaspoon"}, {"item": "ground black pepper", "amount": "1/2 teaspoon"}, {"item": "water", "amount": "3 cups"}], "url": "http://allrecipes.com/Recipe/Navy-Bean-Soup-I/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/01/08/12/1081206.jpg", "ready_in": "4+  hrs", "directions": ["Combine beans, water, tomatoes, onion, celery, garlic, ham, bouillon, Worcestershire sauce, parsley, garlic, and bay leaf in a stock pot; bring to a boil.  Lower heat,  cover, and simmer for two hours.", "Add additional water.  Season with salt and pepper.  Simmer for an additional two hours.  Discard bay leaf."], "description": "\\"A hearty soup that will warm you on a cold night.\\""}', '{"name": "Aunt Jewel\'s Chicken Dressing Casserole", "ingredients": [{"item": "skinless, boneless chicken breast meat", "amount": "3 pounds"}, {"item": "condensed cream of chicken soup", "amount": "1 (10.75 ounce) can"}, {"item": "condensed cream of celery soup", "amount": "1 (10.75 ounce) can"}, {"item": "milk", "amount": "1 (10.75 ounce) can"}, {"item": "chicken broth", "amount": "1 1/2 cups"}, {"item": "seasoned cornbread stuffing mix", "amount": "1 (6 ounce) package"}], "url": "http://allrecipes.com/Recipe/Aunt-Jewels-Chicken-Dressing-Casserole/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/01/11/79/1117965.jpg", "ready_in": "1  hr", "directions": ["Place chicken in a large saucepan full of lightly salted water. Bring to a boil; boil for about 30 minutes, or until chicken is cooked through (juices run clear). Remove chicken from pan, reserving broth. Cut chicken into bite size pieces and place in bottom of a 9x13 inch baking dish.", "Preheat oven to 350 degrees F (175 degrees C).", "In a medium bowl mix together cream of chicken soup and cream of celery soup. Fill one empty soup can with milk, and mix milk with soups. Pour mixture over chicken. In a small bowl combine stuffing and broth; mix together and spoon mixture over casserole.", "Bake in the preheated oven for 45 minutes."], "description": "\\"True Southern chicken and dressing recipe. Handed down from my great-great-aunt. Easy and delicious.\\""}', '{"name": "Cola Chops", "ingredients": [{"item": "pork chops", "amount": "8"}, {"item": "cola-flavored carbonated beverage", "amount": "1 cup"}, {"item": "ketchup", "amount": "1 cup"}, {"item": "brown sugar", "amount": "4 tablespoons"}], "url": "http://allrecipes.com/Recipe/Cola-Chops/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/43/03/430340.jpg", "ready_in": "1\\u00bc  hrs", "directions": ["Preheat oven to 350 degrees F (175 degrees C).", "Place pork chops in a 9x13 inch baking dish. In a small bowl, mix together the cola and ketchup. Pour over chops and sprinkle with brown sugar. Bake uncovered for about 1 hour, or until pork is cooked through and internal temperature has reached 145 degrees F (63 degrees C)."], "description": "\\"Easy, Southern main dish.\\""}', '{"name": "Healthy Garden Salad", "ingredients": [{"item": "red wine vinegar", "amount": "5 tablespoons"}, {"item": "grapeseed oil", "amount": "3 tablespoons"}, {"item": "chopped fresh cilantro", "amount": "1/3 cup"}, {"item": "limes, juiced", "amount": "2"}, {"item": "white sugar", "amount": "1 teaspoon"}, {"item": "salt", "amount": "3/4 teaspoon"}, {"item": "garlic, minced", "amount": "2 cloves"}, {"item": "frozen shelled edamame (green soybeans)", "amount": "1 (1 pound) package"}, {"item": "frozen corn kernels", "amount": "3 cups"}, {"item": "cherry tomatoes, quartered", "amount": "1 pint"}, {"item": "green onions, thinly sliced", "amount": "4"}, {"item": "black beans, rinsed and drained", "amount": "1 (15 ounce) can"}], "url": "http://allrecipes.com/Recipe/Healthy-Garden-Salad/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/60/31/603178.jpg", "ready_in": "", "directions": ["In a large serving bowl, whisk together the red wine vinegar, grapeseed oil, cilantro, lime juice, sugar, salt and garlic. Set aside.", "Bring a large pot of lightly salted water to a boil. Add the soybeans and boil for 3 minutes. Add corn to the boiling water and continue cooking for 1 more minute. Drain very well, and pour into the bowl with the dressing. Gently mix in the cherry tomatoes, green onions and black beans. Cover and refrigerate for at least 2 hours before serving to chill and blend the flavors."], "description": "\\"Edamame (green soybeans), corn, cherry tomatoes and black beans combine to make a colorful salad with a light lime vinaigrette dressing.\\""}', '{"name": "Broccoli Beef II", "ingredients": [{"item": "low-sodium soy sauce", "amount": "2 tablespoons"}, {"item": "fat-free Italian dressing", "amount": "2 tablespoons"}, {"item": "cornstarch", "amount": "1 teaspoon"}, {"item": "minced garlic", "amount": "1 tablespoon"}, {"item": "ground ginger", "amount": "1 teaspoon"}, {"item": "round steak, cut into strips", "amount": "3/4 pound"}, {"item": "water", "amount": "6 cups"}, {"item": "beef bouillon", "amount": "5 cubes"}, {"item": "linguine pasta, uncooked", "amount": "4 ounces"}, {"item": "fat free beef broth", "amount": "1/2 cup"}, {"item": "fresh mushrooms, sliced", "amount": "1 cup"}, {"item": "sliced green onion", "amount": "1/2 cup"}, {"item": "broccoli, separated into florets", "amount": "1 pound"}], "url": "http://allrecipes.com/Recipe/Broccoli-Beef-II/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/46/78/467874.jpg", "ready_in": "1  hr", "directions": ["In a shallow glass dish or bowl, stir together the soy sauce, Italian dressing, cornstarch, garlic and ginger. Place steak strips in the mixture to marinate for 15 minutes.", "While the beef is marinating, combine the bouillon cubes in water in a large saucepan. Bring to a boil, and add pasta. Cook for about 8 minutes, or until al dente. Drain.", "Heat a large skillet over medium-high heat. Remove beef from marinade with a slotted spoon, and place in the hot skillet. Discard marinade. Cook beef, stirring constantly for 2 to 3 minutes, or until mostly browned. Stir in the beef broth, mushrooms and green onions, reduce heat to medium-low, cover, and simmer for about 5 minutes. Remove lid, add broccoli, and cook until broccoli is bright green and tender but still crisp. Add drained linguine, toss, and serve."], "description": "\\"A wonderful dish to make at home and it tastes like take-out. Beef strips are quickly marinated and then sauteed with broccoli and mushrooms in a rich sauce.\\""}', '{"name": "Slow Cooker Chicken Creole", "ingredients": [{"item": "skinless, boneless chicken breast halves", "amount": "4 skinless, boneless chicken breast halves"}, {"item": "salt and pepper to taste", "amount": ""}, {"item": "Creole-style seasoning to taste", "amount": ""}, {"item": "stewed tomatoes, with liquid", "amount": "1 (14.5 ounce) can"}, {"item": "celery, diced", "amount": "1 stalk"}, {"item": "green bell pepper, diced", "amount": "1"}, {"item": "garlic, minced", "amount": "3 cloves"}, {"item": "onion, diced", "amount": "1"}, {"item": "mushrooms, drained", "amount": "1 (4 ounce) can"}, {"item": "fresh jalapeno pepper, seeded and chopped", "amount": "1"}], "url": "http://allrecipes.com/Recipe/Slow-Cooker-Chicken-Creole/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/02/93/29358.jpg", "ready_in": "12+  hrs", "directions": ["Place chicken breasts in slow cooker. Season with salt, pepper, and Creole-style seasoning to taste. Stir in tomatoes with liquid, celery, bell pepper, garlic, onion, mushrooms, and jalapeno pepper.", "Cook on Low for 10 to 12 hours, or on High for 5 to 6 hours."], "description": "\\"The stewed tomatoes and jalapeno pepper give this slow cooker recipe its Creole zing, along with seasoning and other veggies. This is an easy and tasty Creole chicken recipe. Just put all ingredients into the slow cooker and let it simmer all day. It\'s perfect over egg noodles. Add extra water and veggies to the leftovers to make a tasty afternoon soup.\\""}', '{"name": "Pasta with Scallops, Zucchini, and Tomatoes", "ingredients": [{"item": "dry fettuccine pasta", "amount": "1 pound"}, {"item": "olive oil", "amount": "1/4 cup"}, {"item": "garlic, minced", "amount": "3 cloves"}, {"item": "zucchinis, diced", "amount": "2"}, {"item": "salt", "amount": "1/2 teaspoon"}, {"item": "crushed red pepper flakes", "amount": "1/2 teaspoon"}, {"item": "chopped fresh basil", "amount": "1 cup"}, {"item": "roma (plum) tomatoes, chopped", "amount": "4"}, {"item": "bay scallops", "amount": "1 pound"}, {"item": "grated Parmesan cheese", "amount": "2 tablespoons"}], "url": "http://allrecipes.com/Recipe/Pasta-with-Scallops-Zucchini-and-Tomatoes/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/01/12/48/1124847.jpg", "ready_in": "30  mins", "directions": ["In a large pot with boiling salted water cook pasta until al dente. Drain.", "Meanwhile, in a large skillet heat oil, add garlic and cook until tender. Add the zucchini, salt, red pepper flakes, dried basil (if using) and saute for 10 minutes. Add chopped tomatoes, bay scallops, and fresh basil (if using) and simmer for 5 minutes, or until scallops are opaque.", "Pour sauce over cooked pasta and serve with grated Parmesan cheese."], "description": "\\"My family\'s favorite summer meal!\\""}', '{"name": "Cranberry and Cilantro Quinoa Salad", "ingredients": [{"item": "water", "amount": "1 1/2 cups"}, {"item": "uncooked quinoa, rinsed", "amount": "1 cup"}, {"item": "\\u00a0", "amount": ""}, {"item": "red bell pepper, chopped", "amount": "1/4 cup"}, {"item": "yellow bell pepper, chopped", "amount": "1/4 cup"}, {"item": "red onion, finely chopped", "amount": "1 small"}, {"item": "curry powder", "amount": "1 1/2 teaspoons"}, {"item": "chopped fresh cilantro", "amount": "1/4 cup"}, {"item": "lime, juiced", "amount": "1"}, {"item": "toasted sliced almonds", "amount": "1/4 cup"}, {"item": "minced carrots", "amount": "1/2 cup"}, {"item": "dried cranberries", "amount": "1/2 cup"}, {"item": "salt and ground black pepper to taste", "amount": ""}], "url": "http://allrecipes.com/Recipe/Cranberry-and-Cilantro-Quinoa-Salad/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/01/08/11/1081167.jpg", "ready_in": "2\\u00bd  hrs", "directions": ["Pour the water into a saucepan, and cover with a lid. Bring to a boil over high heat, then pour in the quinoa, recover, and continue to simmer over low heat until the water has been absorbed, 15 to 20 minutes. Scrape into a mixing bowl, and chill in the refrigerator until cold.", "Once cold, stir in the red bell pepper, yellow bell pepper, red onion, curry powder, cilantro, lime juice, sliced almonds, carrots, and cranberries. Season to taste with salt and pepper. Chill before serving."], "description": "\\"I got this recipe from the restaurant my dad built.  I love the unique way of cooking the quinoa and the great combinations of the flavors.\\""}', '{"name": "Slow Cooker Chicken Marrakesh", "ingredients": [{"item": "onion, sliced", "amount": "1"}, {"item": "garlic, minced", "amount": "2 cloves"}, {"item": "carrots, peeled and diced", "amount": "2 large"}, {"item": "sweet potatoes, peeled and diced", "amount": "2 large"}, {"item": "garbanzo beans, drained and rinsed", "amount": "1 (15 ounce) can"}, {"item": "skinless, boneless chicken breast halves, cut into 2-inch pieces", "amount": "2 pounds"}, {"item": "ground cumin", "amount": "1/2 teaspoon"}, {"item": "ground turmeric", "amount": "1/2 teaspoon"}, {"item": "ground cinnamon", "amount": "1/4 teaspoon"}, {"item": "ground black pepper", "amount": "1/2 teaspoon"}, {"item": "dried parsley", "amount": "1 teaspoon"}, {"item": "salt", "amount": "1 teaspoon"}, {"item": "diced tomatoes", "amount": "1 (14.5 ounce) can"}], "url": "http://allrecipes.com/Recipe/Slow-Cooker-Chicken-Marrakesh/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/01/04/62/1046254.jpg", "ready_in": "4+  hrs", "directions": ["Place the onion, garlic, carrots, sweet potatoes, garbanzo beans, and chicken breast pieces into a slow cooker. In a bowl, mix the cumin, turmeric, cinnamon, black pepper, parsley, and salt, and sprinkle over the chicken and vegetables. Pour in the tomatoes, and stir to combine.", "Cover the cooker, set to High, and cook until the sweet potatoes are tender and the sauce has thickened, 4 to 5 hours."], "description": "\\"I have been making this Moroccan-style recipe for years. It\'s so easy to make, it has become a tradition in our household.\\""}', '{"name": "Fabulous Fajitas", "ingredients": [{"item": "green bell peppers, sliced", "amount": "2"}, {"item": "red bell pepper, sliced", "amount": "1"}, {"item": "onion, thinly sliced", "amount": "1"}, {"item": "fresh sliced mushrooms", "amount": "1 cup"}, {"item": "diced, cooked chicken meat", "amount": "2 cups"}, {"item": "dry Italian-style salad dressing mix", "amount": "1 (.7 ounce) package"}, {"item": "flour tortillas", "amount": "10 (12 inch)"}], "url": "http://allrecipes.com/Recipe/Fabulous-Fajitas/Detail.aspx?evt19=1", "photo": "http://images.media-allrecipes.com/userphotos/250x250/00/65/60/656089.jpg", "ready_in": "30  mins", "directions": ["Cut peppers and onion into thin slices. Do not dice, leave slices long and thin.", "Saute peppers and onion in a small amount of oil until tender. Add mushrooms and chicken. Continue to cook on low heat until heated through. Stir in dry salad dressing mix and blend thoroughly.", "Warm tortillas and roll mixture inside. If desired top with shredded cheddar cheese, diced tomato and shredded lettuce."], "description": "\\"My kids hate onions and peppers, but they LOVE these fajitas. Pita breads, where available, can be substituted for tortillas.\\""}', '{"name": "Peppered Bacon and Tomato Linguine", "ingredients": [{"item": "peppered bacon, diced", "amount": "1/2 pound"}, {"item": "chopped green onion", "amount": "2 tablespoons"}, {"item": "minced garlic", "amount": "2 teaspoons"}, {"item": "diced tomatoes", "amount": "1 (14.5 ounce) can"}, {"item": "dried basil", "amount": "1 teaspoon"}, {"item": "salt", "amount": "1 teaspoon"}, {"item": "ground black pepper to taste", "amou